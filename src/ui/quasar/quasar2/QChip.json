{
  "name": "QChip",
  "methods": [],
  "events": [
    {
      "name": "click",
      "description": "Emitted on QChip click if 'clickable' property is set",
      "params": "(evt: undefined) => void"
    },
    {
      "name": "update:selected",
      "description": "Used by Vue on 'v-model:selected' for updating its value",
      "params": "(state: Boolean) => void"
    },
    {
      "name": "remove",
      "description": "Works along with 'value' and 'removable' prop. Emitted when toggling rendering state of the QChip",
      "params": "(state: Boolean) => void"
    }
  ],
  "props": {
    "dense": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "dense"
    },
    "size": {
      "value": "",
      "type": "String",
      "description": "QChip size name or a CSS unit including unit name",
      "default": ""
    },
    "dark": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "dark"
    },
    "icon": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "icon"
    },
    "icon-right": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "icon"
    },
    "icon-remove": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "icon"
    },
    "icon-selected": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "icon"
    },
    "label": {
      "value": "",
      "type": [
        "String",
        "Number"
      ],
      "description": "Chip's content as string; overrides default slot if specified",
      "default": ""
    },
    "color": {
      "type": "String",
      "description": "Color name for component from the Quasar Color Palette",
      "value": "",
      "default": "",
      "extends": "color",
      "desc": "Color name for component from the Quasar Color Palette for the inner Spinner (unless using the default slot)",
      "category": "toolbar"
    },
    "text-color": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "text-color"
    },
    "model-value": {
      "value": "",
      "type": "Boolean",
      "description": "",
      "default": true,
      "extends": "model-value",
      "desc": "Model of the component determining if QChip should be rendered or not",
      "required": false
    },
    "selected": {
      "value": "",
      "type": "Boolean",
      "description": "Model for QChip if it's selected or not",
      "default": "false"
    },
    "square": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "square",
      "desc": "Sets a low value for border-radius instead of the default one, making it close to a square"
    },
    "outline": {
      "value": "",
      "type": "Boolean",
      "description": "Display using the 'outline' design",
      "default": "false"
    },
    "clickable": {
      "value": "",
      "type": "Boolean",
      "description": "Is QChip clickable? If it's the case, then it will add hover effects and emit 'click' events",
      "default": "false"
    },
    "removable": {
      "value": "",
      "type": "Boolean",
      "description": "If set, then it displays a 'remove' icon that when clicked the QChip emits 'remove' event",
      "default": "false"
    },
    "ripple": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "ripple"
    },
    "remove-aria-label": {
      "value": "",
      "type": "String",
      "description": "aria-label to be used on the remove icon",
      "default": ""
    },
    "tabindex": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "tabindex"
    },
    "disable": {
      "value": "",
      "type": "String",
      "description": "",
      "default": "",
      "extends": "disable"
    }
  },
  "link": "https://v2.quasar.dev/vue-components/chip"
}
