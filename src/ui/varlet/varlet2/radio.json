{
  "name": "VarRadio",
  "props": {
    "v-model": {
      "value": "",
      "description": "绑定的值",
      "default": "false",
      "type": "any"
    },
    "checked-value": {
      "value": "",
      "description": "选中状态的值",
      "default": "true",
      "type": "any"
    },
    "unchecked-value": {
      "value": "",
      "description": "未选中状态的值",
      "default": "false",
      "type": "any"
    },
    "checked-color": {
      "value": "",
      "description": "选中状态的颜色",
      "default": "-",
      "type": "any"
    },
    "unchecked-color": {
      "value": "",
      "description": "未选中状态的颜色",
      "default": "-",
      "type": "any"
    },
    "icon-size": {
      "value": "",
      "description": "图标尺寸",
      "default": "-",
      "type": "string | number"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "readonly": {
      "value": "",
      "description": "是否只读",
      "default": "false",
      "type": "boolean"
    },
    "ripple": {
      "value": "",
      "description": "是否开启水波纹",
      "default": "true",
      "type": "boolean"
    },
    "rules": {
      "value": "",
      "description": "验证规则，返回 `true` 表示验证通过，其余的值则转换为文本作为用户提示",
      "default": "-",
      "type": "Array<(value: any) => any>"
    }
  },
  "methods": [
    {
      "name": "validate",
      "description": "触发校验",
      "params": "`-`"
    },
    {
      "name": "resetValidation",
      "description": "清空校验信息",
      "params": "`-`"
    },
    {
      "name": "reset",
      "description": "清空绑定的值(设置为 `unchecked-value`)和校验信息",
      "params": "`-`"
    },
    {
      "name": "toggle",
      "description": "切换选中状态，传 `checked-value` 为选中，`unchecked-value` 为取消选中，不传或其他情况为取反",
      "params": "`value: any`"
    }
  ],
  "typeDetail": {},
  "events": [
    {
      "name": "click",
      "description": "点击时触发",
      "params": "`e: Event`"
    },
    {
      "name": "change",
      "description": "状态变更时触发",
      "params": "`value: any`"
    }
  ]
}
