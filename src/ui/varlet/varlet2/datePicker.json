{
  "name": "VarDatePicker",
  "props": {
    ":v-model": {
      "value": "",
      "description": "被选择的日期（ISO 8601 格式，`YYYY-MM-DD` 或 `YYYY-MM`）",
      "default": "undefined",
      "type": "string[] | string",
      "required": true
    },
    "type": {
      "value": ["date", "month"],
      "description": "选择器类型，可选值为 `date month`",
      "default": "date",
      "type": "string"
    },
    "allowed-dates": {
      "value": "",
      "description": "限制可以选择的日期",
      "default": "-",
      "type": "function"
    },
    "color": {
      "value": "",
      "description": "选择器的颜色",
      "default": "#2979ff",
      "type": "string"
    },
    "header-color": {
      "value": "",
      "description": "标题背景色。如果未指定，将使用由 color 属性或默认颜色。",
      "default": "#2979ff",
      "type": "string"
    },
    "elevation": {
      "value": ["true", "false", "0-24"],
      "description": "海拔高度，可选值为 `true` `false` 和 `0-24` 的等级",
      "default": "false",
      "type": "string | number | boolean"
    },
    "button-elevation": {
      "value": "",
      "description": "Button 的海拔高度",
      "default": "true",
      "type": "string | number | boolean"
    },
    "first-day-of-week": {
      "value": "",
      "description": "设置一周的第一天，从周日的 0 开始。",
      "default": "0",
      "type": "string | number"
    },
    "min": {
      "value": "",
      "description": "允许的最小日期/月份（ISO 8601格式）",
      "default": "-",
      "type": "string"
    },
    "max": {
      "value": "",
      "description": "允许的最大日期/月份（ISO 8601格式）",
      "default": "-",
      "type": "string"
    },
    "show-current": {
      "value": "",
      "description": "是否显示当前日期",
      "default": "true",
      "type": "boolean"
    },
    "readonly": {
      "value": "",
      "description": "是否只读",
      "default": "false",
      "type": "boolean"
    },
    "multiple": {
      "value": "",
      "description": "是否支持选择多个日期",
      "default": "false",
      "type": "boolean"
    },
    "range": {
      "value": "",
      "description": "是否支持选择一个范围",
      "default": "false",
      "type": "boolean"
    },
    "touchable": {
      "value": "",
      "description": "是否支持拖动切换面板",
      "default": "true",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "preview",
      "description": "日期切换时触发",
      "params": "year: number, month: number"
    },
    {
      "name": "change",
      "description": "日期改变时触发",
      "params": "日value: string | string[]"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/date-picker"
}
