{
  "name": "VarUploader",
  "props": {
    "v-model": {
      "value": "",
      "description": "文件列表",
      "default": "[]",
      "type": "VarFile[]",
      "required":true
    },
    "accept": {
      "value": [
        "image",
        "*"
      ],
      "description": "接受的文件类型，与原生属性一致",
      "default": "image | *",
      "type": "string"
    },
    "capture": {
      "value": "",
      "description": "获取文件方式，与原生属性一致",
      "default": "-",
      "type": "string"
    },
    "multiple": {
      "value": "",
      "description": "是否多选文件",
      "default": "false",
      "type": "boolean"
    },
    "readonly": {
      "value": "",
      "description": "是否只读",
      "default": "false",
      "type": "boolean"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "elevation": {
      "value": ["true", "false", "0-24"],
      "description": "海拔高度，可选值为 `true` `false` 和 `0-24` 的等级, 不为简单模式时生效",
      "default": "true",
      "type": "string | number | boolean"
    },
    "removable": {
      "value": "",
      "description": "是否可以删除",
      "default": "true",
      "type": "boolean"
    },
    "maxlength": {
      "value": "",
      "description": "最大文件个数",
      "default": "-",
      "type": "string | number"
    },
    "maxsize": {
      "value": "",
      "description": "最大文件大小",
      "default": "-",
      "type": "string | number"
    },
    "previewed": {
      "value": "",
      "description": "是否允许预览",
      "default": "true",
      "type": "boolean"
    },
    "ripple": {
      "value": "",
      "description": "是否开启水波纹",
      "default": "true",
      "type": "boolean"
    },
    "hide-list": {
      "value": "",
      "description": "是否隐藏文件列表",
      "default": "false",
      "type": "boolean"
    },
    "validate-trigger": {
      "value": ["onChange", "onRemove"],
      "description": "触发验证的时机， 可选值为 `onChange` onRemove",
      "default": "['onChange', 'onRemove']",
      "type": "ValidateTriggers[]"
    },
    "rules": {
      "value": "",
      "description": "验证规则，返回 `true` 表示验证通过，其余的值则转换为文本作为用户提示",
      "default": "-",
      "type": "Array<(v: VarFile, u: VarFileUtils) => any>"
    }
  },
  "methods": [{ "name": "chooseFile", "description": "触发选择文件动作，显示文件列表", "params": "`-`" }, { "name": "closePreview", "description": "关闭预览文件弹出层", "params": "`-`" }, { "name": "getLoading", "description": "获取 `state` 等于 `loading` 的文件集合", "params": "`-`" }, { "name": "getSuccess", "description": "获取 `state` 等于 `success` 的文件集合", "params": "`-`" }, { "name": "getError", "description": "获取 `state` 等于 `error` 的文件集合", "params": "`-`" }, { "name": "validate", "description": "触发校验", "params": "`-`" }, { "name": "resetValidation", "description": "清空校验信息", "params": "`-`" }, { "name": "reset", "description": "清空绑定的值(设置为 `[]`)和校验信息", "params": "`-`" }],
  "typeDetail": {
    "VarFile": [
      {
        "name": "file",
        "description": "原生文件",
        "type": "File"
      },
      {
        "name": "name",
        "description": "文件名",
        "type": "string"
      },
      {
        "name": "url",
        "description": "文件地址",
        "type": "string"
      },
      {
        "name": "cover",
        "description": "封面图",
        "type": "string"
      },
      {
        "name": "fit",
        "description": "封面图填充模式，可选值为 `fill` `contain` `cover` `none` `scale-down`",
        "type": "string"
      },
      {
        "name": "state",
        "description": "文件上传状态，可选值为 `loading` `success` error",
        "type": "string"
      },
      {
        "name": "progress",
        "description": "文件上传进度，范围 [0, 100]",
        "type": "number"
      }
    ],
    "VarFileUtils": [
      {
        "name": "getLoading",
        "description": "获取 `state` 等于 `loading` 的文件集合",
        "type": "`-`"
      },
      {
        "name": "getSuccess",
        "description": "获取 `state` 等于 `success` 的文件集合",
        "type": "`-`"
      },
      {
        "name": "getError",
        "description": "获取 `state` 等于 `error` 的文件集合",
        "type": "`-`"
      }
    ]
  },
  "events": [
    {
      "name": "before-filter",
      "description": "`before-read` 前触发，对文件列表进行过滤",
      "params": "files: VarFile[]"
    },
    {
      "name": "before-read",
      "description": "文件读取前触发，返回假值阻止文件读取(支持 promise)",
      "params": "files: VarFile[]"
    },
    {
      "name": "after-read",
      "description": "文件读取后触发",
      "params": "files: VarFile[]"
    },
    {
      "name": "oversize",
      "description": "文件超过限制大小时触发",
      "params": "files: VarFile[]"
    },
    {
      "name": "before-remove",
      "description": "文件删除前触发，返回假值阻止文件删除(支持 promise)",
      "params": "files: VarFile[]"
    },
    {
      "name": "remove",
      "description": "文件删除时触发",
      "params": "files: VarFile[]"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/uploader"
}
