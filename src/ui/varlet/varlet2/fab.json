    },
    {
      "name": "open",
      "description": "在激活并打开动作菜单时触发",
      "params": "_"
    },
    {
      "name": "opened",
      "description": "在激活并打开动作菜单的动画结束时触发",
      "params": "_"
    },
    {
      "name": "close",
      "description": "在失活并关闭动作菜单时触发",
      "params": "_"
    },
    {
      "name": "closed",
      "description": "在失活并关闭动作菜单的动画结束时触发",
      "params": "_"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/fab"
}
