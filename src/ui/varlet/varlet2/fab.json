{
  "name": "VarFab",
  "props": {
    "v-model:active": {
      "value": "",
      "description": "是否激活动作菜单",
      "default": "false",
      "type": "boolean",
      "required":true
    },
    "show": {
      "value": "",
      "description": "是否显示触发器",
      "default": "true",
      "type": "boolean"
    },
    "type": {
      "value": ["default", "primary", "info", "success", "warning", "danger"],
      "description": "类型，可选值为 `default` `primary` `info` `success` `warning` danger",
      "default": "primary",
      "type": "string"
    },
    "position": {
      "value": ["left-top", "right-top", "left-bottom", "right-bottom"],
      "description": "触发器位置，可选值为 `left-top` `right-top` `left-bottom` `right-bottom`",
      "default": "right-bottom",
      "type": "string"
    },
    "direction": {
      "value": ["top", "bottom", "left", "right"],
      "description": "动作菜单弹出方向，可选值为 `top` `bottom` `left` right",
      "default": "top",
      "type": "string"
    },
    "trigger": {
      "value": ["click", "hover"],
      "description": "触发方式 `click` hover",
      "default": "click",
      "type": "string"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "color": {
      "value": "",
      "description": "触发器颜色",
      "default": "-",
      "type": "string"
    },
    "inactive-icon": {
      "value": "",
      "description": "未激活时触发器图标",
      "default": "plus",
      "type": "string"
    },
    "active-icon": {
      "value": "",
      "description": "激活时触发器图标",
      "default": "window-close",
      "type": "string"
    },
    "inactive-icon-size": {
      "value": "",
      "description": "未激活时触发器图标尺寸",
      "default": "-",
      "type": "string"
    },
    "active-icon-size": {
      "value": "",
      "description": "激活时触发器图标尺寸",
      "default": "-",
      "type": "string"
    },
    "fixed": {
      "value": "",
      "description": "是否使用固定定位，设置为 `false` 可开启绝对定位",
      "default": "true",
      "type": "boolean"
    },
    "z-index": {
      "value": "",
      "description": "同 css z-index，用于自定义触发器层级",
      "default": "90",
      "type": "number"
    },
    "top": {
      "value": "",
      "description": "同 css top，用于自定义触发器位置",
      "default": "-",
      "type": "string"
    },
    "bottom": {
      "value": "",
      "description": "同 css bottom，用于自定义触发器位置",
      "default": "-",
      "type": "string"
    },
    "left": {
      "value": "",
      "description": "同 css left，用于自定义触发器位置",
      "default": "-",
      "type": "string"
    },
    "right": {
      "value": "",
      "description": "同 css right，用于自定义触发器位置",
      "default": "-",
      "type": "string"
    },
    "safe-area": {
      "value": "",
      "description": "是否开启底部安全区适配",
      "default": "false",
      "type": "boolean"
    },
    "teleport": {
      "value": "",
      "description": "组件挂载的元素",
      "default": "-",
      "type": "TeleportProps['to'] | false"
    },
    "elevation": {
      "value": ["true", "false", "0-24"],
      "description": "海拔高度，可选值为 `true`、`false` 和 `0-24` 的等级",
      "default": "true",
      "type": "string | number | boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "click",
      "description": "点击触发器时触发，在 `disabled` 状态为 `true` 时不触发",
