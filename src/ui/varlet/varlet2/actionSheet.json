{
  "name": "VarActionSheet",
  "props": {
    "v-model:show": {
      "value": "",
      "description": "是否显示动作面板",
      "default": "false",
      "type": "boolean"
    },
    "actions": {
      "value": "",
      "description": "选项列表",
      "default": "[]",
      "type": "Actions"
    },
    "title": {
      "value": "",
      "description": "动作面板标题",
      "default": "请选择",
      "type": "string"
    },
    "overlay": {
      "value": "",
      "description": "是否显示遮罩层",
      "default": "true",
      "type": "boolean"
    },
    "overlay-class": {
      "value": "",
      "description": "自定义遮罩层的 class",
      "default": "-",
      "type": "string"
    },
    "overlay-style": {
      "value": "",
      "description": "自定义遮罩层的 style",
      "default": "-",
      "type": "object"
    },
    "lock-scroll": {
      "value": "",
      "description": "是否禁止滚动穿透，禁止时滚动弹出层不会引发 body 的滚动",
      "default": "true",
      "type": "boolean"
    },
    "close-on-click-action": {
      "value": "",
      "description": "是否点击选项时关闭动作面板",
      "default": "true",
      "type": "boolean"
    },
    "close-on-click-overlay": {
      "value": "",
      "description": "是否点击遮罩层关闭动作面板",
      "default": "true",
      "type": "boolean"
    },
    "safe-area": {
      "value": "",
      "description": "是否开启底部安全区适配",
      "default": "false",
      "type": "boolean"
    },
    "teleport": {
      "value": "",
      "description": "动作面板挂载的位置",
      "default": "body",
      "type": "TeleportProps['to'] | false"
    }
  },
  "methods": [
    {
      "name": "ActionSheet",
      "description": "显示动作面板",
      "params": "options"
    },
    {
      "name": "ActionSheet.close",
      "description": "关闭动作面板",
      "params": "-"
    },
    {
      "name": "ActionSheet.setDefaultOptions",
      "description": "设置默认的选项配置",
      "params": "options"
    },
    {
      "name": "ActionSheet.resetDefaultOptions",
      "description": "重置默认的选项配置",
      "params": "-"
    }
  ],
