{
  "name": "VarMenu",
  "props": {
    "v-model:show": {
      "value": "",
      "description": "是否显示菜单",
      "default": "default",
      "type": "string",
      "required":true
    },
    "placement": {
      "value": "",
      "description": "菜单弹出位置",
      "default": "cover-top-start",
      "type": "Placement"
    },
    "strategy": {
      "value": ["absolute", "fixed"],
      "description": "菜单定位方式，可选值为 absolute fixed",
      "default": "absolute",
      "type": "string"
    },
    "offset-x": {
      "value": "",
      "description": "x 轴偏移量， 相对于菜单对齐后的位置",
      "default": "0",
      "type": "number | string"
    },
    "offset-y": {
      "value": "",
      "description": "y 轴偏移量， 相对于菜单对齐后的位置",
      "default": "0",
      "type": "number | string"
    },
    "teleport": {
      "value": "",
      "description": "菜单挂载的位置",
      "default": "body",
      "type": "TeleportProps['to'] | false"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用菜单",
      "default": "false",
      "type": "boolean"
    },
    "trigger": {
      "value": ["click", "hover"],
      "description": "菜单触发方式，可选值为 `click` hover, `click` 为点击时触发, `hover` 为悬停时触发",
      "default": "click",
      "type": "string"
    },
    "reference": {
      "value": "",
      "description": "菜单关联的触发元素选择器，用于指定菜单的特定子元素为触发元素",
      "default": "-",
      "type": "string"
    },
    "elevation": {
      "value": ["true", "false", "0-24"],
      "description": "海拔高度，可选值为 `true` `false` 和 `0-24` 的等级",
      "default": "true",
      "type": "string | number | boolean"
    },
    "same-width": {
      "value": "",
      "description": "是否与触发元素同宽",
      "default": "false",
      "type": "boolean"
    },
    "default-style": {
      "value": "",
      "description": "是否启用默认样式",
      "default": "true",
      "type": "boolean"
    },
    "popover-class": {
      "value": "",
      "description": "弹出层的 class",
      "default": "-",
      "type": "string"
    },
    "close-on-click-reference": {
      "value": "",
      "description": "是否在点击菜单触发元素后关闭菜单",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [{ "name": "open", "description": "打开 Menu", "params": "`-`" }, { "name": "close", "description": "关闭 Menu", "params": "`-`" }, { "name": "resize", "description": "Menu 默认插槽元素产生位置大小变化时可以调用此方法进行重绘", "params": "`-`" }],
