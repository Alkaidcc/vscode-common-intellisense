{
  "name": "VarTooltip",
  "props": {
    "v-model:show": {
      "value": "",
      "description": "是否显示",
      "default": "false",
      "type": "boolean"
    },
    "content": {
      "value": "",
      "description": "显示的内容",
      "default": "-",
      "type": "string"
    },
    "color": {
      "value": "",
      "description": "背景颜色",
      "default": "-",
      "type": "string"
    },
    "type": {
      "value": ["primary", "info", "success", "warning", "danger", "default"],
      "description": "类型，可选值为 `default` `primary` `info` `success` `warning` `danger`",
      "default": "default",
      "type": "string"
    },
    "placement": {
      "value": ["top", "bottom", "left", "right"],
      "description": "弹出位置",
      "default": "top",
      "type": "Placement"
    },
    "offset-x": {
      "value": "",
      "description": "x 轴偏移量， 相对于 `Tooltip` 对齐后的位置",
      "default": "0",
      "type": "number | string"
    },
    "offset-y": {
      "value": "",
      "description": "y 轴偏移量， 相对于 `Tooltip` 对齐后的位置",
      "default": "0",
      "type": "number | string"
    },
    "teleport": {
      "value": "",
      "description": "挂载的位置",
      "default": "body",
      "type": "TeleportProps['to'] | false"
    },
    "same-width": {
      "value": "",
      "description": "是否与触发元素同宽",
      "default": "false",
      "type": "boolean"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用 Tooltip",
      "default": "false",
      "type": "boolean"
    },
    "trigger": {
      "value": ["click", "hover"],
      "description": "触发方式，可选值为 `click` `hover`, `click` 为点击时触发, `hover` 为悬停时触发",
      "default": "hover",
      "type": "string"
    },
    "reference": {
      "value": "",
      "description": "关联的触发元素选择器，用于指定特定子元素为触发元素",
      "default": "-",
      "type": "string"
    },
    "close-on-click-reference": {
      "value": "",
      "description": "是否在点击菜单触发元素后关闭菜单",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [{ "name": "open", "description": "打开 `Tooltip`", "params": "`-`" }, { "name": "close", "description": "关闭 `Tooltip`", "params": "`-`" }, { "name": "resize", "description": "`Tooltip` 默认插槽元素产生位置大小变化时可以调用此方法进行重绘", "params": "`-`" }, { "name": "click-outside", "description": "点击菜单外部时触发", "params": "`event: Event`" }],
  "typeDetail": {
    "$Placement": [
      {
        "name": "`top`",
        "description": "顶部中心位置",
        "type": ""
      },
      {
        "name": "`top-start`",
        "description": "顶部左侧位置",
        "type": ""
      },
      {
        "name": "`top-end`",
        "description": "顶部右侧位置",
        "type": ""
      },
      {
        "name": "`bottom`",
        "description": "底部中心位置",
        "type": ""
      },
      {
        "name": "`bottom-start`",
        "description": "底部左侧位置",
        "type": ""
      },
      {
        "name": "`bottom-end`",
        "description": "底部右侧位置",
        "type": ""
      },
      {
        "name": "`right`",
        "description": "右侧中心位置",
        "type": ""
      },
      {
        "name": "`right-start`",
        "description": "右侧上方位置",
        "type": ""
      },
      {
        "name": "`right-end`",
        "description": "右侧下方位置",
        "type": ""
      },
      {
        "name": "`left`",
        "description": "左侧中心位置",
        "type": ""
      },
      {
        "name": "`left-start`",
        "description": "左侧上方位置",
        "type": ""
      },
      {
        "name": "`left-end`",
        "description": "左侧下方位置",
        "type": ""
      }
    ]
  },
  "events": [
    {
      "name": "open",
      "description": "打开 `Tooltip` 时触发",
      "params": "_"
    },
    {
      "name": "opened",
      "description": "打开 `Tooltip` 动画结束时触发",
      "params": "_"
    },
    {
      "name": "close",
      "description": "关闭 `Tooltip` 时触发",
      "params": "_"
    },
    {
      "name": "closed",
      "description": "关闭 `Tooltip` 动画结束时触发",
      "params": "_"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/tooltip"
}
