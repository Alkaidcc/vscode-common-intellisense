{
  "name": "VarPopup",
  "props": {
    "v-model:show": {
      "value": "",
      "description": "是否显示弹出层",
      "default": "false",
      "type": "boolean"
    },
    "position": {
      "value": ["top", "bottom", "right", "left", "center"],
      "description": "弹出位置，可选值为 `top` `bottom` `right` `left` center",
      "default": "center",
      "type": "string"
    },
    "overlay": {
      "value": "",
      "description": "是否显示遮罩层",
      "default": "true",
      "type": "boolean"
    },
    "overlay-class": {
      "value": "",
      "description": "自定义遮罩层的 class",
      "default": "-",
      "type": "string"
    },
    "overlay-style": {
      "value": "",
      "description": "自定义遮罩层的 style",
      "default": "-",
      "type": "string"
    },
    "default-style": {
      "value": "",
      "description": "是否启用默认样式",
      "default": "true",
      "type": "boolean"
    },
    "transition": {
      "value": "",
      "description": "过度动画的名称",
      "default": "-",
      "type": "string"
    },
    "lock-scroll": {
      "value": "",
      "description": "是否禁止滚动穿透，禁止时滚动弹出层不会引发 body 的滚动",
      "default": "true",
      "type": "boolean"
    },
    "close-on-click-overlay": {
      "value": "",
      "description": "是否点击遮罩层关闭弹出层",
      "default": "true",
      "type": "boolean"
    },
    "teleport": {
      "value": "",
      "description": "弹出层挂载的位置",
      "default": "body",
      "type": "TeleportProps['to'] | false"
    },
    "safe-area": {
      "value": "",
      "description": "是否开启底部安全区适配",
      "default": "false",
      "type": "boolean"
    },
    "safe-area-top": {
      "value": "",
      "description": "是否开启顶部安全区适配",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "open",
      "description": "打开弹出层时触发",
      "params": "_"
    },
    {
      "name": "opened",
      "description": "打开弹出层动画结束时触发",
      "params": "_"
    },
    {
      "name": "close",
      "description": "关闭弹出层时触发",
      "params": "_"
    },
    {
      "name": "closed",
      "description": "关闭弹出层动画结束时触发",
      "params": "_"
    },
    {
      "name": "click-overlay",
      "description": "点击遮罩层时触发",
      "params": "_"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/popup"
}
