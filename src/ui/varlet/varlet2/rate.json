{
  "name": "VarRate",
  "props": {
    "v-model": {
      "value": "",
      "description": "当前分数",
      "default": "0",
      "type": "当前分数"
    },
    "count": {
      "value": "",
      "description": "评分总数",
      "default": "5",
      "type": "评分总数"
    },
    "color": {
      "value": "",
      "description": "选中时图标的颜色",
      "default": "-",
      "type": "选中时图标的颜色"
    },
    "icon": {
      "value": "",
      "description": "选中整个图标时的图标样式",
      "default": "star",
      "type": "选中整个图标时的图标样式"
    },
    "empty-color": {
      "value": "",
      "description": "未选中时图标的颜色",
      "default": "-",
      "type": "未选中时图标的颜色"
    },
    "empty-icon": {
      "value": "",
      "description": "未选中时图标的样式",
      "default": "star-outline",
      "type": "未选中时图标的样式"
    },
    "size": {
      "value": "",
      "description": "图标大小，默认单位为 `px`",
      "default": "-",
      "type": "图标大小，默认单位为 px"
    },
    "gap": {
      "value": "",
      "description": "图标间隔，默认单位为 `px`",
      "default": "-",
      "type": "图标间隔，默认单位为 px"
    },
    "half": {
      "value": "",
      "description": "是否允许半选",
      "default": "false",
      "type": "是否允许半选"
    },
    "half-icon": {
      "value": "",
      "description": "半选时图标的样式，只有在 `half` 为 `true` 时才有效",
      "default": "star-half-full",
      "type": "半选时图标的样式，只有在 half 为 true 时才有效"
    },
    "namespace": {
      "value": "",
      "description": "图标的命名空间, 可扩展自定义图标库",
      "default": "var-icon",
      "type": "图标的命名空间, 可扩展自定义图标库"
    },
    "disabled": {
      "value": "",
      "description": "是否禁止评分",
      "default": "false",
      "type": "是否禁止评分"
    },
    "disabled-color": {
      "value": "",
      "description": "禁止评分时图标的颜色，只有在 `disabled` 为 `true` 时才有效，优先级高于 `color`、`empty-color`",
      "default": "#aaa",
      "type": "禁止评分时图标的颜色，只有在 disabled 为 true 时才有效，优先级高于 color、empty-color"
    },
    "readonly": {
      "value": "",
      "description": "是否使用评分只读",
      "default": "false",
      "type": "是否使用评分只读"
    },
    "clearable": {
      "value": "",
      "description": "是否允许清空",
      "default": "false",
      "type": "是否允许清空"
    },
    "ripple": {
      "value": "",
      "description": "是否使用水波纹",
      "default": "true",
      "type": "是否使用水波纹"
    },
    "rules": {
      "value": "",
      "description": "验证规则，返回 `true` 表示验证通过，其余的值则转换为文本作为用户提示",
      "default": "-",
      "type": "验证规则，返回 true 表示验证通过，其余的值则转换为文本作为用户提示"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "评分改变时触发",
      "params": "评分改变时触发"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/rate"
}
