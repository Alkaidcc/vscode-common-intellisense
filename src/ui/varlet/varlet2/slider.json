{
  "name": "VarSlider",
  "props": {
    "v-model": {
      "value": "",
      "description": "当前进度值",
      "default": "0",
      "type": "number | [number, number]",
      "required":true
    },
    "step": {
      "value": "",
      "description": "步长，取值必须大于 0",
      "default": "1",
      "type": "string | number"
    },
    "range": {
      "value": "",
      "description": "是否启用双滑块",
      "default": "false",
      "type": "boolean"
    },
    "max": {
      "value": "",
      "description": "最大值",
      "default": "100",
      "type": "string | number"
    },
    "min": {
      "value": "",
      "description": "最小值",
      "default": "0",
      "type": "string | number"
    },
    "label-visible": {
      "value": ["always", "normal", "never"],
      "description": "是否显示标签，可选值为 `always normal never`",
      "default": "normal",
      "type": "string"
    },
    "label-text-color": {
      "value": "",
      "description": "标签文字颜色",
      "default": "#fff",
      "type": "string"
    },
    "label-color": {
      "value": "",
      "description": "标签颜色",
      "default": "-",
      "type": "string"
    },
    "active-color": {
      "value": "",
      "description": "已激活的轨道背景颜色",
      "default": "-",
      "type": "string"
    },
    "track-color": {
      "value": "",
      "description": "轨道背景颜色",
      "default": "-",
      "type": "string"
    },
    "track-height": {
      "value": "",
      "description": "轨道的高度",
      "default": "2",
      "type": "string | number"
    },
    "thumb-size": {
      "value": "",
      "description": "滑块的大小",
      "default": "12",
      "type": "string | number"
    },
    "thumb-color": {
      "value": "",
      "description": "滑块的背景颜色",
      "default": "-",
      "type": "string"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "readonly": {
      "value": "",
      "description": "是否只读",
      "default": "false",
      "type": "boolean"
    },
    "direction": {
      "value": ["horizontal", "vertical"],
      "description": "显示方向，可选值为 `vertical horizontal`",
      "default": "horizontal",
      "type": "string"
    },
    "rules": {
      "value": "",
      "description": "校验规则",
      "default": "-",
      "type": "array"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "值改变时触发",
      "params": "value: 当前进度"
    },
    {
      "name": "start",
      "description": "开始拖动时触发",
      "params": "_"
    },
    {
      "name": "end",
      "description": "结束拖动时触发",
      "params": "value: 当前进度"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/slider"
}
