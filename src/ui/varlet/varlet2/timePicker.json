{
  "name": "VarTimePicker",
  "props": {
    "v-model": {
      "value": "",
      "description": "被选择的时间（ISO 8601 格式，`HH:mm` 或 `HH:mm:ss`）",
      "default": "-",
      "type": "string",
      "required":true
    },
    "format": {
      "value": ["ampm", "24hr"],
      "description": "选择器时间格式，可选值为 `ampm 24hr`",
      "default": "ampm",
      "type": "string"
    },
    "color": {
      "value": "",
      "description": "选择器的颜色",
      "default": "#2979ff",
      "type": "string"
    },
    "header-color": {
      "value": "",
      "description": "标题背景色。如果未指定，将使用由 color 属性或默认颜色。",
      "default": "#2979ff",
      "type": "string"
    },
    "elevation": {
      "value": ["true", "false", "0-24"],
      "description": "海拔高度，可选值为 `true` `false` 和 `0-24` 的等级",
      "default": "false",
      "type": "string | number | boolean"
    },
    "min": {
      "value": "",
      "description": "允许的最小时间（ISO 8601格式）",
      "default": "-",
      "type": "string"
    },
    "max": {
      "value": "",
      "description": "允许的最大时间（ISO 8601格式）",
      "default": "-",
      "type": "string"
    },
    "allowed-time": {
      "value": "",
      "description": "限制可以选择的时间",
      "default": "-",
      "type": "AllowedTime"
    },
    "readonly": {
      "value": "",
      "description": "是否只读",
      "default": "false",
      "type": "boolean"
    },
    "use-seconds": {
      "value": "",
      "description": "是否显示秒",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {
    "AllowedTime": [
      {
        "name": "hours",
        "description": "限制可选的 hour",
        "type": "Function: hour => boolean"
      },
      {
        "name": "minutes",
        "description": "限制可选的 minute",
        "type": "Function: minute => boolean"
      },
      {
        "name": "seconds",
        "description": "限制可选的 second",
        "type": "Function: second => boolean"
      }
    ]
  },
  "events": [
    {
      "name": "change",
      "description": "时间变化时触发",
      "params": "value: string"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/time-picker"
}
