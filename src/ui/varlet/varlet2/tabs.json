{
  "name": "VarTabs",
  "props": {
    "`v-model:active`": {
      "value": "",
      "description": "激活的选项卡标识, 优先匹配 name，其次是 index",
      "default": "default",
      "type": "string"
    },
    "layout-direction": {
      "value": ["horizontal", "vertical"],
      "description": "选项卡组的布局方向，可选值为 `horizontal` `vertical`",
      "default": "horizontal",
      "type": "string"
    },
    "item-direction": {
      "value": ["horizontal", "vertical"],
      "description": "选项卡的布局方向，可选值为 `horizontal` `vertical`",
      "default": "horizontal",
      "type": "string"
    },
    "fixed-bottom": {
      "value": "",
      "description": "是否固定在窗口底部, 可用做底部导航",
      "default": "false",
      "type": "boolean"
    },
    "color": {
      "value": "",
      "description": "选项卡的背景颜色",
      "default": "#fff",
      "type": "string"
    },
    "active-color": {
      "value": "",
      "description": "选项卡激活的主要颜色",
      "default": "#2979ff",
      "type": "string"
    },
    "inactive-color": {
      "value": "",
      "description": "选项卡未激活的主要颜色",
      "default": "#646566",
      "type": "string"
    },
    "disabled-color": {
      "value": "",
      "description": "选项卡禁用时的主要颜色",
      "default": "#e0e0e0",
      "type": "string"
    },
    "indicator-color": {
      "value": "",
      "description": "选项卡激活指示器的颜色",
      "default": "#2979ff",
      "type": "string"
    },
    "indicator-size": {
      "value": "",
      "description": "选项卡激活指示器的尺寸",
      "default": "2px",
      "type": "string | number"
    },
    "elevation": {
      "value": ["true", "false", "0-24"],
      "description": "海拔高度，可选值为 `true` `false` 和 `0-24` 的等级",
      "default": "false",
      "type": "string |number | boolean"
    },
    "sticky": {
      "value": "",
      "description": "是否启用粘性布局",
      "default": "false",
      "type": "boolean"
    },
    "sticky-css-mode": {
      "value": "",
      "description": "是否启用粘性布局的原生 css sticky 模式",
      "default": "false",
      "type": "boolean"
    },
    "sticky-z-index": {
      "value": "",
      "description": "粘性布局的层级",
      "default": "10",
      "type": "number"
    },
    "safe-area": {
      "value": "",
      "description": "是否开启底部安全区适配",
      "default": "false",
      "type": "boolean"
    },
    "offset-top": {
      "value": "",
      "description": "吸顶距离",
      "default": "0",
      "type": "string | number"
    },
    "scrollable": {
      "value": ["auto", "always"],
      "description": "是否开启滚动，可选值为 `auto` `always`",
      "default": "auto",
      "type": "string"
    },
    "indicator-position": {
      "value": ["normal", "reverse"],
      "description": "选项卡激活指示器的位置，可选值为 `normal` `reverse`",
      "default": "normal",
      "type": "string"
    }
  },
  "methods": [{ "name": "resize", "description": "产生位置大小变化时可以调用此方法重绘选项卡组", "params": "`-`" }, { "name": "resizeSticky", "description": "产生位置大小变化时可以调用此方法重绘粘性布局", "params": "`-`" }],
  "typeDetail": {},
  "events": [
    {
      "name": "`click`",
      "description": "点击选项卡时触发, 在选项卡 `disabled` 状态为 `true` 时不触发",
      "params": "active: string | number"
    },
    {
      "name": "`change`",
      "description": "切换选项卡时触发",
      "params": "active: string | number"
    }
  ],
  "link": "https://varlet.gitee.io/varlet-ui/#/zh-CN/tabs"
}
