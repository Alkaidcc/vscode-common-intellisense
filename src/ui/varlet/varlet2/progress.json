{
  "name": "VarProgress",
  "props": {
    "mode": {
      "value": ["linear", "circle"],
      "description": "progress 的模式，可选值为 `linear, circle`",
      "default": "linear",
      "type": "string"
    },
    "type": {
      "value": ["primary", "info", "success", "warning", "danger"],
      "description": "类型，可选值为 `default` `primary` `info` `success` `warning` danger",
      "default": "primary",
      "type": "string"
    },
    "value": {
      "value": "",
      "description": "progress 的进度",
      "default": "0",
      "type": "string | number"
    },
    "line-width": {
      "value": "",
      "description": "progress 的线宽",
      "default": "4",
      "type": "string | number"
    },
    "color": {
      "value": "",
      "description": "progress 的颜色",
      "default": "#005CAF",
      "type": "string"
    },
    "track-color": {
      "value": "",
      "description": "progress 轨道的颜色",
      "default": "#d8d8d8",
      "type": "string"
    },
    "label": {
      "value": "",
      "description": "是否显示 label",
      "default": "false",
      "type": "boolean"
    },
    "label-class": {
      "value": "",
      "description": "自定义 `label` 的类名",
      "default": "-",
      "type": "string"
    },
    "track": {
      "value": "",
      "description": "是否显示 `progress` 的轨道",
