      "type": "boolean"
    },
    "indeterminate": {
      "value": "",
      "description": "是否显示不间断动画",
      "default": "false",
      "type": "boolean"
    },
    "size": {
      "value": "",
      "description": "progress 的尺寸（仅支持环形进度条）",
      "default": "40",
      "type": "string | number"
    },
    "rotate": {
      "value": "",
      "description": "progress 的原点（仅支持环形进度条）",
      "default": "0",
      "type": "number"
    }
