{
  "name": "Tooltip",
  "props": {
    "title": {
      "value": "",
      "description": "提示文字",
      "default": "-",
      "type": "ReactNode | () => ReactNode"
    },
    "align": {
      "value": "",
      "description": "该值将合并到 placement 的配置中",
      "default": "-",
      "type": "object"
    },
    "arrow": {
      "value": "",
      "description": "修改箭头的显示状态以及修改箭头是否指向目标元素中心",
      "default": "true",
      "type": "boolean | { pointAtCenter: boolean }"
    },
    "autoAdjustOverflow": {
      "value": "",
      "description": "气泡被遮挡时自动调整位置",
      "default": "true",
      "type": "boolean"
    },
    "color": {
      "value": "",
      "description": "背景颜色",
      "default": "-",
      "type": "string"
    },
    "defaultOpen": {
      "value": "",
      "description": "默认是否显隐",
      "default": "false",
      "type": "boolean"
    },
    "destroyTooltipOnHide": {
      "value": "",
      "description": "关闭后是否销毁 Tooltip",
      "default": "false",
      "type": "boolean"
    },
    "getPopupContainer": {
      "value": "",
      "description": "浮层渲染父节点，默认渲染到 body 上",
      "default": "() => document.body",
      "type": "(triggerNode: HTMLElement) => HTMLElement"
    },
    "mouseEnterDelay": {
      "value": "",
      "description": "鼠标移入后延时多少才显示 Tooltip，单位：秒",
      "default": "0.1",
      "type": "number"
    },
    "mouseLeaveDelay": {
      "value": "",
      "description": "鼠标移出后延时多少才隐藏 Tooltip，单位：秒",
      "default": "0.1",
      "type": "number"
    },
    "overlayClassName": {
      "value": "",
      "description": "卡片类名",
      "default": "-",
      "type": "string"
    },
    "overlayStyle": {
      "value": "",
      "description": "卡片样式",
      "default": "-",
      "type": "object"
    },
    "overlayInnerStyle": {
      "value": "",
      "description": "卡片内容区域的样式对象",
      "default": "-",
      "type": "object"
    },
    "placement": {
      "value": "",
      "description": "气泡框位置，可选 `top` `left` `right` `bottom` `topLeft` `topRight` `bottomLeft` `bottomRight` `leftTop` `leftBottom` `rightTop` `rightBottom`",
      "default": "`top`",
      "type": "string"
    },
    "trigger": {
      "value": "",
      "description": "触发行为，可选 `hover` | `focus` | `click` | `contextMenu`，可使用数组设置多个触发行为",
      "default": "`hover`",
      "type": "string | string[]"
    },
    "open": {
      "value": "",
      "description": "用于手动控制浮层显隐，小于 4.23.0 使用 `visible`",
      "default": "false",
      "type": "boolean"
    },
    "zIndex": {
      "value": "",
      "description": "设置 Tooltip 的 `z-index`",
      "default": "-",
      "type": "number"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "onOpenChange",
      "description": "显示隐藏的回调",
      "params": "(open: boolean) => void"
    }
  ]
}
