{
  "name": "DatePicker",
  "props": {
    "allowClear": {
      "value": "",
      "description": "是否显示清除按钮",
      "default": "true",
      "type": "boolean"
    },
    "autoFocus": {
      "value": "",
      "description": "自动获取焦点",
      "default": "false",
      "type": "boolean"
    },
    "bordered": {
      "value": "",
      "description": "是否有边框",
      "default": "true",
      "type": "boolean"
    },
    "className": {
      "value": "",
      "description": "选择器 className",
      "default": "-",
      "type": "string"
    },
    "dateRender": {
      "value": "",
      "description": "自定义日期单元格的内容，5.4.0 起用 `cellRender` 代替",
      "default": "-",
      "type": "function(currentDate: dayjs, today: dayjs) => React.ReactNode"
    },
    "changeOnBlur": {
      "value": "",
      "description": "失去焦点时触发 `change` 事件，例如 datetime 下不再需要点击确认按钮",
      "default": "false",
      "type": "boolean"
    },
    "cellRender": {
      "value": "",
      "description": "自定义单元格的内容",
      "default": "-",
      "type": "(current: dayjs, info: { originNode: React.ReactElement,today: DateType, range?: 'start' | 'end', type: PanelMode, locale?: Locale, subType?: 'hour' | 'minute' | 'second' | 'meridiem' }) => React.ReactNode"
    },
    "disabled": {
      "value": "",
      "description": "禁用",
      "default": "false",
      "type": "boolean"
    },
    "disabledDate": {
      "value": "",
      "description": "不可选择的日期",
      "default": "-",
      "type": "(currentDate: dayjs) => boolean"
    },
    "format": {
      "value": [
        "[rc-picker]"
      ],
      "description": "设置日期格式，为数组时支持多格式匹配，展示以第一个为准",
      "default": "[rc-picker]",
      "type": "[formatType]"
    },
    "popupClassName": {
      "value": "",
      "description": "额外的弹出日历 className",
      "default": "-",
      "type": "string"
    },
    "getPopupContainer": {
      "value": "",
      "description": "定义浮层的容器，默认为 body 上新建 div",
      "default": "-",
      "type": "function(trigger)"
    },
    "inputReadOnly": {
      "value": "",
      "description": "设置输入框为只读（避免在移动设备上打开虚拟键盘）",
      "default": "false",
      "type": "boolean"
    },
    "locale": {
      "value": [
        "[默认配置]"
      ],
      "description": "国际化配置",
      "default": "[默认配置]",
      "type": "object"
    },
    "mode": {
      "value": ["time", "date", "month", "year", "decade"],
      "description": "日期面板的状态",
      "default": "-",
      "type": "time | date | month | year | decade"
    },
    "nextIcon": {
      "value": "",
      "description": "自定义下一个图标",
      "default": "-",
      "type": "ReactNode"
    },
    "open": {
      "value": "",
      "description": "控制弹层是否展开",
      "default": "false",
      "type": "boolean"
    },
    "panelRender": {
      "value": "",
      "description": "自定义渲染面板",
      "default": "-",
      "type": "(panelNode) => ReactNode"
    },
    "picker": {
      "value": ["date", "week", "month", "quarter", "year"],
      "description": "设置选择器类型",
      "default": "date",
      "type": "date | week | month | quarter | year"
    },
    "placeholder": {
      "value": "",
      "description": "输入框提示文字",
      "default": "-",
      "type": "string | [string, string]"
    },
    "placement": {
      "value": ["bottomLeft", "bottomRight", "topLeft", "topRight"],
      "description": "选择框弹出的位置",
      "default": "bottomLeft",
      "type": "bottomLeft `bottomRight` `topLeft` topRight"
    },
    "popupStyle": {
      "value": "",
      "description": "额外的弹出日历样式",
      "default": "{}",
      "type": "CSSProperties"
    },
    "prevIcon": {
      "value": "",
      "description": "自定义上一个图标",
      "default": "-",
      "type": "ReactNode"
    },
    "presets": {
      "value": "",
      "description": "预设时间范围快捷选择",
      "default": "-",
      "type": "{ label: React.ReactNode, value: [dayjs][]"
    },
    "size": {
      "value": ["large", "middle", "small"],
      "description": "输入框大小，`large` 高度为 40px，`small` 为 24px，默认是 32px",
      "default": "-",
      "type": "large | middle | small"
    },
    "status": {
      "value": ["error", "warning"],
      "description": "设置校验状态",
      "default": "-",
      "type": "'error' | 'warning'"
    },
    "style": {
      "value": "",
      "description": "自定义输入框样式",
      "default": "{}",
      "type": "CSSProperties"
    },
    "suffixIcon": {
      "value": "",
      "description": "自定义的选择框后缀图标",
      "default": "-",
      "type": "ReactNode"
    },
    "superNextIcon": {
      "value": "",
      "description": "自定义 `>>` 切换图标",
      "default": "-",
      "type": "ReactNode"
    },
    "superPrevIcon": {
      "value": "",
      "description": "自定义 `<<` 切换图标",
      "default": "-",
      "type": "ReactNode"
    },
    "defaultPickerValue": {
      "value": "",
      "description": "默认面板日期",
      "default": "-",
      "type": "[dayjs]"
    },
    "defaultValue": {
      "value": "",
      "description": "默认日期，如果开始时间或结束时间为 `null` 或者 `undefined`，日期范围将是一个开区间",
      "default": "-",
      "type": "[dayjs]"
    },
    "disabledTime": {
      "value": "",
      "description": "不可选择的时间",
      "default": "-",
      "type": "function(date)"
    },
    "renderExtraFooter": {
      "value": "",
      "description": "在面板中添加额外的页脚",
      "default": "-",
      "type": "(mode) => React.ReactNode"
    },
    "showNow": {
      "value": "",
      "description": "当设定了 `showTime` 的时候，面板是否显示“此刻”按钮",
      "default": "false",
      "type": "boolean"
    },
    "showTime": {
      "value": ["[TimePicker Options]"],
      "description": "增加时间选择功能",
      "default": "[TimePicker Options]",
      "type": "Object | boolean"
    },
    "showTime.defaultValue": {
      "value": "",
      "description": "设置用户选择日期时默认的时分秒",
      "default": "dayjs()",
      "type": "[dayjs]"
    },
    "showToday": {
      "value": "",
      "description": "是否展示“今天”按钮",
      "default": "true",
      "type": "boolean"
    },
    "value": {
      "value": "",
      "description": "日期",
      "default": "-",
      "type": "[dayjs]"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "onOpenChange",
      "description": "弹出日历和关闭日历的回调",
      "params": "function(open)"
    },
    {
      "name": "onPanelChange",
      "description": "日历面板切换的回调",
      "params": "function(value, mode)"
    }
  ],
  "link": "https://ant.design/components//date-picker"
}
