{
  "name": "Cascader",
  "props": {
    "allowClear": {
      "value": "",
      "description": "是否支持清除",
      "default": "true",
      "type": "boolean"
    },
    "autoFocus": {
      "value": "",
      "description": "自动获取焦点",
      "default": "false",
      "type": "boolean"
    },
    "bordered": {
      "value": "",
      "description": "是否有边框",
      "default": "true",
      "type": "boolean"
    },
    "clearIcon": {
      "value": "",
      "description": "自定义的选择框清空图标",
      "default": "-",
      "type": "ReactNode"
    },
    "changeOnSelect": {
      "value": "",
      "description": "（单选时生效）当此项为 true 时，点选每级菜单选项值都会发生变化，具体见上面的演示",
      "default": "false",
      "type": "boolean"
    },
    "className": {
      "value": "",
      "description": "自定义类名",
      "default": "-",
      "type": "string"
    },
    "defaultValue": {
      "value": "",
      "description": "默认的选中项",
      "default": "[]",
      "type": "string[] | number[]"
    },
    "disabled": {
      "value": "",
      "description": "禁用",
      "default": "false",
      "type": "boolean"
    },
    "displayRender": {
      "value": ["label => label.join(`", "`)"],
      "description": "选择后展示的渲染函数",
      "default": "label => label.join(` | `)",
      "type": "(label, selectedOptions) => ReactNode"
    },
    "tagRender": {
      "value": "",
      "description": "自定义 tag 内容 render，仅在多选时生效",
      "default": "-",
      "type": "({ label: string, onClose: function, value: string }) => ReactNode"
    },
    "popupClassName": {
      "value": "",
      "description": "自定义浮层类名",
      "default": "-",
      "type": "string"
    },
    "dropdownRender": {
      "value": "",
      "description": "自定义下拉框内容",
      "default": "-",
      "type": "(menus: ReactNode) => ReactNode"
    },
    "expandIcon": {
      "value": "",
      "description": "自定义次级菜单展开图标",
      "default": "-",
      "type": "ReactNode"
    },
    "expandTrigger": {
      "value": "",
      "description": "次级菜单的展开方式，可选 'click' 和 'hover'",
      "default": "`click`",
      "type": "string"
    },
    "fieldNames": {
      "value": "",
      "description": "自定义 options 中 label value children 的字段",
      "default": "{ label: `label`, value: `value`, children: `children` }",
      "type": "object"
    },
    "getPopupContainer": {
      "value": "",
      "description": "菜单渲染父节点。默认渲染到 body 上，如果你遇到菜单滚动定位问题，试试修改为滚动的区域，并相对其定位。[示例](https://codepen.io/afc163/pen/zEjNOy?editors=0010)",
      "default": "() => document.body",
      "type": "function(triggerNode)"
    },
    "loadData": {
      "value": "",
      "description": "用于动态加载选项，无法与 `showSearch` 一起使用",
      "default": "-",
      "type": "(selectedOptions) => void"
    },
    "maxTagCount": {
      "value": "",
      "description": "最多显示多少个 tag，响应式模式会对性能产生损耗",
      "default": "-",
      "type": "number | `responsive`"
    },
    "maxTagPlaceholder": {
      "value": "",
      "description": "隐藏 tag 时显示的内容",
      "default": "-",
      "type": "ReactNode | function(omittedValues)"
    },
    "maxTagTextLength": {
      "value": "",
      "description": "最大显示的 tag 文本长度",
      "default": "-",
      "type": "number"
    },
    "notFoundContent": {
      "value": "",
      "description": "当下拉列表为空时显示的内容",
      "default": "`Not Found`",
      "type": "string"
    },
    "open": {
      "value": "",
      "description": "控制浮层显隐",
      "default": "false",
      "type": "boolean"
    },
    "options": {
      "value": "",
      "description": "可选项数据源",
      "default": "-",
      "type": "[Option][]"
    },
    "placeholder": {
      "value": "",
      "description": "输入框占位文本",
      "default": "`请选择`",
      "type": "string"
    },
    "placement": {
      "value": "",
      "description": "浮层预设位置",
      "default": "`bottomLeft`",
      "type": "`bottomLeft` `bottomRight` `topLeft` `topRight`"
    },
    "showSearch": {
      "value": "",
      "description": "在选择框中显示搜索框",
      "default": "false",
      "type": "boolean | [Object]"
    },
    "size": {
      "value": "",
      "description": "输入框大小",
      "default": "-",
      "type": "`large` | `middle` | `small`"
    },
    "status": {
      "value": "",
      "description": "设置校验状态",
      "default": "-",
      "type": "'error' | 'warning'"
    },
    "style": {
      "value": "",
      "description": "自定义样式",
      "default": "-",
      "type": "CSSProperties"
    },
    "suffixIcon": {
      "value": "",
      "description": "自定义的选择框后缀图标",
      "default": "-",
      "type": "ReactNode"
    },
    "value": {
      "value": "",
      "description": "指定选中项",
      "default": "-",
      "type": "string[] | number[]"
    },
    "multiple": {
      "value": "",
      "description": "支持多选节点",
      "default": "false",
      "type": "boolean"
    },
    "showCheckedStrategy": {
      "value": "",
      "description": "定义选中项回填的方式。`Cascader.SHOW_CHILD`: 只显示选中的子节点。`Cascader.SHOW_PARENT`: 只显示父节点（当父节点下所有子节点都选中时）。",
      "default": "`Cascader.SHOW_PARENT`",
      "type": "`Cascader.SHOW_PARENT` | `Cascader.SHOW_CHILD`"
    },
    "removeIcon": {
      "value": "",
      "description": "自定义的多选框清除图标",
      "default": "-",
      "type": "ReactNode"
    },
    "searchValue": {
      "value": "",
      "description": "设置搜索的值，需要与 `showSearch` 配合使用",
      "default": "-",
      "type": "string"
    },
    "dropdownMenuColumnStyle": {
      "value": "",
      "description": "下拉菜单列的样式",
      "default": "-",
      "type": "CSSProperties"
    },
    "filter": {
      "value": "",
      "description": "接收 `inputValue` `path` 两个参数，当 `path` 符合筛选条件时，应返回 true，反之则返回 false",
      "default": "-",
      "type": "function(inputValue, path): boolean"
    },
    "limit": {
      "value": "",
      "description": "搜索结果展示数量",
      "default": "50",
      "type": "number | false"
    },
    "matchInputWidth": {
      "value": "",
      "description": "搜索结果列表是否与输入框同宽（[效果](https://github.com/ant-design/ant-design/issues/25779)）",
      "default": "true",
      "type": "boolean"
    },
    "render": {
      "value": "",
      "description": "用于渲染 filter 后的选项",
      "default": "-",
      "type": "function(inputValue, path): ReactNode"
    },
    "sort": {
      "value": "",
      "description": "用于排序 filter 后的选项",
      "default": "-",
      "type": "function(a, b, inputValue)"
    }
  },
  "typeDetail": {
    "objectItem": [
      {
        "name": "filter",
        "description": "接收 inputValue path 两个参数，当 path 符合筛选条件时，应返回 true，反之则返回 false",
        "type": "function(inputValue, path): boolean"
      },
      {
        "name": "limit",
        "description": "搜索结果展示数量",
        "type": "number | false"
      },
      {
        "name": "matchInputWidth",
        "description": "搜索结果列表是否与输入框同宽",
        "type": "boolean"
      },
      {
        "name": "render",
        "description": "用于渲染 filter 后的选项",
        "type": "function(inputValue, path): ReactNode"
      },
      {
        "name": "sort",
        "description": "用于排序 filter 后的选项",
        "type": "function(a, b, inputValue)"
      }
    ],
    "optionsItem": [
      {
        "name": "value",
        "type": "string | number"
      },
      {
        "name": "label",
        "type": "React.ReactNode"
      },
      {
        "disabled": "value",
        "type": "boolean"
      },
      {
        "children": "value",
        "type": "optionsItem[]"
      },
      {
        "isLeaf": "value",
        "description": "设为 `false` 时会强制标记为父节点，即使当前节点没有 children，也会显示展开图标",
        "type": "boolean"
      }
    ]
  },
  "methods": [{ "name": "blur()", "description": "移除焦点", "params": "" }, { "name": "focus()", "description": "获取焦点", "params": "" }],
  "events": [
    {
      "name": "onChange",
      "description": "选择完成后的回调",
      "params": "(value, selectedOptions) => void"
    },
    {
      "name": "onDropdownVisibleChange",
      "description": "显示/隐藏浮层的回调",
      "params": "(value) => void"
    },
    {
      "name": "onSearch",
      "description": "监听搜索，返回输入的值",
      "params": "(search: string) => void"
    }
  ]
}
