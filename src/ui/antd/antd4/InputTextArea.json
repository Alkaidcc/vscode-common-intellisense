{
  "name": "InputTextArea",
  "props": {
    "allowClear": {
      "value": "",
      "description": "可以点击清除图标删除内容",
      "default": "false",
      "type": "boolean"
    },
    "autoSize": {
      "value": "",
      "description": "自适应内容高度，可设置为 true | false 或对象：{ minRows: 2, maxRows: 6 }",
      "default": "false",
      "type": "boolean | object"
    },
    "bordered": {
      "value": "",
      "description": "是否有边框",
      "default": "true",
      "type": "boolean"
    },
    "classNames": {
      "value": "",
      "description": "语义化结构 class",
      "default": "-",
      "type": "Record<[SemanticDOM], string>"
    },
    "defaultValue": {
      "value": "",
      "description": "输入框默认内容",
      "default": "-",
      "type": "string"
    },
    "maxLength": {
      "value": "",
      "description": "内容最大长度",
      "default": "-",
      "type": "number"
    },
    "showCount": {
      "value": "",
      "description": "是否展示字数",
      "default": "false",
      "type": "boolean | { formatter: (info: { value: string, count: number, maxLength?: number }) => string }"
    },
    "styles": {
      "value": "",
      "description": "语义化结构 style",
      "default": "-",
      "type": "Record<[SemanticDOM], CSSProperties>"
    },
    "value": {
      "value": "",
      "description": "输入框内容",
      "default": "-",
      "type": "string"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "onPressEnter",
      "description": "按下回车的回调",
      "params": "function(e)"
    },
    {
      "name": "onResize",
      "description": "resize 回调",
      "params": "function({ width, height })"
    }
  ],
  "link": "https://ant.design/components//input"
}
