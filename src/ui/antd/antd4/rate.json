{
  "name": "Rate",
  "props": {
    "allowClear": {
      "value": "",
      "description": "是否允许再次点击后清除",
      "default": "true",
      "type": "boolean"
    },
    "allowHalf": {
      "value": "",
      "description": "是否允许半选",
      "default": "false",
      "type": "boolean"
    },
    "autoFocus": {
      "value": "",
      "description": "自动获取焦点",
      "default": "false",
      "type": "boolean"
    },
    "character": {
      "value": ["<StarFilled />"],
      "description": "自定义字符",
      "default": "<StarFilled />",
      "type": "ReactNode | (RateProps) => ReactNode"
    },
    "className": {
      "value": "",
      "description": "自定义样式类名",
      "default": "-",
      "type": "string"
    },
    "count": {
      "value": "",
      "description": "star 总数",
      "default": "5",
      "type": "number"
    },
    "defaultValue": {
      "value": "",
      "description": "默认值",
      "default": "0",
      "type": "number"
    },
    "disabled": {
      "value": "",
      "description": "只读，无法进行交互",
      "default": "false",
      "type": "boolean"
    },
    "style": {
      "value": "",
      "description": "自定义样式对象",
      "default": "-",
      "type": "CSSProperties"
    },
    "tooltips": {
      "value": "",
      "description": "自定义每项的提示信息",
      "default": "-",
      "type": "string[]"
    },
    "value": {
      "value": "",
      "description": "当前数，受控值",
      "default": "-",
      "type": "number"
    }
  },
  "methods": [{ "name": "blur()", "description": "移除焦点", "params": "" }, { "name": "focus()", "description": "获取焦点", "params": "" }],
  "events": [
    {
      "name": "onBlur",
      "description": "失去焦点时的回调",
      "params": "function()"
    },
    {
      "name": "onChange",
      "description": "选择时的回调",
      "params": "function(value: number)"
    },
    {
      "name": "onFocus",
      "description": "获取焦点时的回调",
      "params": "function()"
    },
    {
      "name": "onHoverChange",
      "description": "鼠标经过时数值变化的回调",
      "params": "function(value: number)"
    },
    {
      "name": "onKeyDown",
      "description": "按键回调",
      "params": "function(event)"
    }
  ],
  "link": "http://192.168.119.22:8001/components/rate"
}
