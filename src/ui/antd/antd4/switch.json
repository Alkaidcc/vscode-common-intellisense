{
  "name": "Switch",
  "props": {
    "autoFocus": {
      "value": "",
      "description": "组件自动获取焦点",
      "default": "false",
      "type": "boolean"
    },
    "checked": {
      "value": "",
      "description": "指定当前是否选中",
      "default": "false",
      "type": "boolean"
    },
    "checkedChildren": {
      "value": "",
      "description": "选中时的内容",
      "default": "-",
      "type": "ReactNode"
    },
    "className": {
      "value": "",
      "description": "Switch 器类名",
      "default": "-",
      "type": "string"
    },
    "defaultChecked": {
      "value": "",
      "description": "初始是否选中",
      "default": "false",
      "type": "boolean"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "loading": {
      "value": "",
      "description": "加载中的开关",
      "default": "false",
      "type": "boolean"
    },
    "size": {
      "value": "",
      "description": "开关大小，可选值：`default` `small`",
      "default": "`default`",
      "type": "string"
    },
    "unCheckedChildren": {
      "value": "",
      "description": "非选中时的内容",
      "default": "-",
      "type": "ReactNode"
    }
  },
  "methods": [{ "name": "blur()", "description": "移除焦点", "params": "" }, { "name": "focus()", "description": "获取焦点", "params": "" }],
  "events": [
    {
      "name": "onChange",
      "description": "变化时的回调函数",
      "params": "function(checked: boolean, event: Event)"
    },
    {
      "name": "onClick",
      "description": "点击时的回调函数",
      "params": "function(checked: boolean, event: Event)"
    }
  ],
  "link": "https://ant.design/components//switch"
}
