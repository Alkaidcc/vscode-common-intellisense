{
  "name": "Collapse",
  "props": {
    "accordion": {
      "value": "",
      "description": "手风琴模式",
      "default": "false",
      "type": "boolean"
    },
    "activeKey": {
      "value": "",
      "description": "当前激活 tab 面板的 key",
      "default": "默认无，accordion 模式下默认第一个元素",
      "type": "string[] | string |  number[] | number"
    },
    "bordered": {
      "value": "",
      "description": "带边框风格的折叠面板",
      "default": "true",
      "type": "boolean"
    },
    "collapsible": {
      "value": ["header", "icon", "disabled"],
      "description": "所有子面板是否可折叠或指定可折叠触发区域",
      "default": "-",
      "type": "header | icon | disabled"
    },
    "defaultActiveKey": {
      "value": "",
      "description": "初始化选中面板的 key",
      "default": "-",
      "type": "string[] | string | number[] | number"
    },
    "destroyInactivePanel": {
      "value": "",
      "description": "销毁折叠隐藏的面板",
      "default": "false",
      "type": "boolean"
    },
    "expandIcon": {
      "value": "",
      "description": "自定义切换图标",
      "default": "-",
      "type": "(panelProps) => ReactNode"
    },
    "expandIconPosition": {
      "value": ["start", "end"],
      "description": "设置图标位置",
      "default": "-",
      "type": "start | end"
    },
    "ghost": {
      "value": "",
      "description": "使折叠面板透明且无边框",
      "default": "false",
      "type": "boolean"
    },
    "size": {
      "value": ["large", "middle", "small"],
      "description": "设置折叠面板大小",
      "default": "middle",
      "type": "large | middle | small"
    },
    ":items": {
      "value": "",
      "description": "折叠项目内容",
      "default": "-",
      "type": "[ItemType]"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "onChange",
      "description": "切换面板的回调",
      "params": "function"
    }
  ],
  "link": "https://ant.design/components//collapse"
}
