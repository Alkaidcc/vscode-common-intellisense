{
  "name": "Carousel",
  "props": {
    "autoplay": {
      "value": "",
      "description": "是否自动切换",
      "default": "false",
      "type": "boolean"
    },
    "dotPosition": {
      "value": "",
      "description": "面板指示点位置，可选 `top` `bottom` `left` `right`",
      "default": "`bottom`",
      "type": "string"
    },
    "dots": {
      "value": "",
      "description": "是否显示面板指示点，如果为 `object` 则同时可以指定 `dotsClass` 或者",
      "default": "true",
      "type": "boolean | { className?: string }"
    },
    "waitForAnimate": {
      "value": "",
      "description": "是否等待切换动画",
      "default": "false",
      "type": "boolean"
    },
    "easing": {
      "value": "",
      "description": "动画效果",
      "default": "`linear`",
      "type": "string"
    },
    "effect": {
      "value": "",
      "description": "动画效果函数",
      "default": "`scrollx`",
      "type": "`scrollx` | `fade`"
    },
    "afterChange": {
      "value": "",
      "description": "切换面板的回调",
      "default": "-",
      "type": "(current: number) => void"
    },
    "beforeChange": {
      "value": "",
      "description": "切换面板的回调",
      "default": "-",
      "type": "(current: number, next: number) => void"
    }
  },
  "methods": [{ "name": "goTo(slideNumber, dontAnimate)", "description": "切换到指定面板, dontAnimate = true 时，不使用动画", "params": "" }, { "name": "next()", "description": "切换到下一面板", "params": "" }, { "name": "prev()", "description": "切换到上一面板", "params": "" }],
  "events": [],
  "link": "http://192.168.119.22:8001/components/carousel"
}
