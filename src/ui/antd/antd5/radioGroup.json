{
  "name": "RadioGroup",
  "props": {
    "buttonStyle": {
      "value": "",
      "description": "RadioButton 的风格样式，目前有描边和填色两种风格",
      "default": "`outline`",
      "type": "`outline` | `solid`"
    },
    "defaultValue": {
      "value": "",
      "description": "默认选中的值",
      "default": "-",
      "type": "any"
    },
    "disabled": {
      "value": "",
      "description": "禁选所有子单选器",
      "default": "false",
      "type": "boolean"
    },
    "name": {
      "value": "",
      "description": "RadioGroup 下所有 `input[type=\"radio\"]` 的 `name` 属性",
      "default": "-",
      "type": "string"
    },
    "options": {
      "value": "",
      "description": "以配置形式设置子元素",
      "default": "-",
      "type": "string\\[] | number\\[] | Array<{ label: ReactNode; value: string; disabled?: boolean; }>"
    },
    "optionType": {
      "value": "",
      "description": "用于设置 Radio `options` 类型",
      "default": "`default`",
      "type": "`default` | `button`"
    },
    "size": {
      "value": "",
      "description": "大小，只对按钮样式生效",
      "default": "-",
      "type": "`large` | `middle` | `small`"
    },
    "value": {
      "value": "",
      "description": "用于设置当前选中的值",
      "default": "-",
      "type": "any"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "onChange",
      "description": "选项变化时的回调函数",
      "params": "function(e:Event)"
    }
  ],
  "link": "https://ant.design/components//radio"
}
