{
  "name": "Space",
  "props": {
    "align": {
      "value": [
        "start",
        "end",
        "center",
        "baseline"
      ],
      "description": "对齐方式",
      "default": "—",
      "type": "start | end | center | baseline"
    },
    "direction": {
      "value": [
        "vertical",
        "horizontal"
      ],
      "description": "间距方向",
      "default": "horizontal",
      "type": "vertical | horizontal"
    },
    "size": {
      "value": [
        "small",
        "middle",
        "large",
        "number"
      ],
      "description": "间距大小",
      "default": "small",
      "type": "small | middle | large | number"
    },
    "split": {
      "value": "",
      "description": "设置拆分",
      "default": "—",
      "type": "ReactNode"
    },
    "wrap": {
      "value": "",
      "description": "是否自动换行，仅在 horizontal 时有效",
      "default": "false",
      "type": "boolean"
    }
  }
}
