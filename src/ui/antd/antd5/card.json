{
  "name": "Card",
  "props": {
    "actions": {
      "value": "",
      "description": "卡片操作组，位置在卡片底部",
      "default": "-",
      "type": "Array<ReactNode>"
    },
    "activeTabKey": {
      "value": "",
      "description": "当前激活页签的 key",
      "default": "-",
      "type": "string"
    },
    "bodyStyle": {
      "value": "",
      "description": "内容区域自定义样式",
      "default": "-",
      "type": "CSSProperties"
    },
    "bordered": {
      "value": "",
      "description": "是否有边框",
      "default": "true",
      "type": "boolean"
    },
    "cover": {
      "value": "",
      "description": "卡片封面",
      "default": "-",
      "type": "ReactNode"
    },
    "defaultActiveTabKey": {
      "value": "",
      "description": "初始化选中页签的 key，如果没有设置 activeTabKey",
      "default": "",
      "type": "string"
    },
    "extra": {
      "value": "",
      "description": "卡片右上角的操作区域",
      "default": "-",
      "type": "ReactNode"
    },
    "headStyle": {
      "value": "",
      "description": "自定义标题区域样式",
      "default": "-",
      "type": "CSSProperties"
    },
    "hoverable": {
      "value": "",
      "description": "鼠标移过时可浮起",
      "default": "false",
      "type": "boolean"
    },
    "loading": {
      "value": "",
      "description": "当卡片内容还在加载中时，可以用 loading 展示一个占位",
      "default": "false",
      "type": "boolean"
    },
    "size": {
      "value": ["default", "small"],
      "description": "card 的尺寸",
      "default": "default",
      "type": "default | small"
    },
    "tabBarExtraContent": {
      "value": "",
      "description": "tab bar 上额外的元素",
      "default": "-",
      "type": "ReactNode"
    },
    "tabList": {
      "value": "",
      "description": "页签标题列表",
      "default": "-",
      "type": "[TabItemType][]"
    },
    "tabProps": {
      "value": "",
      "description": "[Tabs]",
      "default": "-",
      "type": "-"
    },
    "title": {
      "value": "",
      "description": "卡片标题",
      "default": "-",
      "type": "ReactNode"
    },
    "type": {
      "value": "",
      "description": "卡片类型，可设置为 `inner` 或 不设置",
      "default": "-",
      "type": "string"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "onTabChange",
      "description": "页签切换的回调",
      "params": "(key) => void"
    }
  ],
  "link": "https://ant.design/components//card"
}
