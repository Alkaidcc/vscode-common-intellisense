{
  "name": "ElPopover",
  "props": {
    "trigger": {
      "value": [
        "click",
        "focus",
        "hover",
        "manual"
      ],
      "description": "触发方式",
      "default": "click | focus | hover | manual",
      "type": "String"
    },
    "title": {
      "value": "",
      "description": "标题",
      "default": "—",
      "type": "String"
    },
    "content": {
      "value": "",
      "description": "显示的内容，也可以通过 `slot` 传入 DOM",
      "default": "—",
      "type": "String"
    },
    "width": {
      "value": "",
      "description": "宽度",
      "default": "—",
      "type": "String, Number"
    },
    "placement": {
      "value": [
        "top",
        "top-start",
        "top-end",
        "bottom",
        "bottom-start",
        "bottom-end",
        "left",
        "left-start",
        "left-end",
        "right",
        "right-start",
        "right-end"
      ],
      "description": "出现位置",
      "default": "top | top-start | top-end | bottom | bottom-start | bottom-end | left | left-start | left-end | right | right-start | right-end",
      "type": "String"
    },
    "disabled": {
      "value": "",
      "description": "Popover 是否可用",
      "default": "—",
      "type": "Boolean"
    },
    "value": {
      "value": "",
      "description": "状态是否可见",
      "default": "—",
      "type": "Boolean"
    },
    "v-model": {
      "value": "",
      "description": "状态是否可见",
      "default": "—",
      "type": "Boolean"
    },
    "offset": {
      "value": "",
      "description": "出现位置的偏移量",
      "default": "—",
      "type": "Number"
    },
    "transition": {
      "value": "",
      "description": "定义渐变动画",
      "default": "—",
      "type": "String"
    },
    "visible-arrow": {
      "value": "",
      "description": "是否显示 Tooltip 箭头，更多参数可见[Vue-popper](https://github.com/element-component/vue-popper)",
      "default": "—",
      "type": "Boolean"
    },
    ":popper-options": {
      "value": "",
      "description": "[popper.js](https://popper.js.org/docs/v2/) 的参数",
      "default": "{ boundariesElement: 'body', gpuAcceleration: false }",
      "type": "Object"
    },
    "popper-class": {
      "value": "",
      "description": "为 popper 添加类名",
      "default": "—",
      "type": "String"
    },
    "open-delay": {
      "value": "",
      "description": "触发方式为 hover 时的显示延迟，单位为毫秒",
      "default": "—",
      "type": "Number"
    },
    "close-delay": {
      "value": "",
      "description": "触发方式为 hover 时的隐藏延迟，单位为毫秒",
      "default": "—",
      "type": "number"
    },
    "tabindex": {
      "value": "",
      "description": "Popover 组件的 [tabindex](https://developer.mozilla.org/en-US/docs/Web/HTML/Global_attributes/tabindex)",
      "default": "—",
      "type": "number"
    }
  },
  "events": [
    {
      "name": "show",
      "description": "显示时触发",
      "params": "—"
    },
    {
      "name": "after-enter",
      "description": "显示动画播放完毕后触发",
      "params": "—"
    },
    {
      "name": "hide",
      "description": "隐藏时触发",
      "params": "—"
    },
    {
      "name": "after-leave",
      "description": "隐藏动画播放完毕后触发",
      "params": "—"
    }
  ],
  "link": "https://element.eleme.cn/#/zh-CN/component/popover"
}
