    "icon": {
      "value": "",
      "description": "设置头像的图标类型，参考 Icon 组件",
      "default": "",
      "type": "string"
    },
    "size": {
      "value": [
        "number",
        "large",
        "medium",
        "small"
      ],
      "description": "设置头像的大小",
      "default": "number | large | medium | small",
