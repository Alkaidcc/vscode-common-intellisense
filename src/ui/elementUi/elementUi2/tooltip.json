{
  "name": "ElTooltip",
  "props": {
    "effect": {
      "value": [
        "dark",
        "light"
      ],
      "description": "默认提供的主题",
      "default": "dark | light",
      "type": "String"
    },
    "content": {
      "value": "",
      "description": "显示的内容，也可以通过 `slot#content` 传入 DOM",
      "default": "—",
      "type": "String"
    },
    "placement": {
      "value": [
        "top",
        "top-start",
        "top-end",
        "bottom",
        "bottom-start",
        "bottom-end",
        "left",
        "left-start",
        "left-end",
        "right",
        "right-start",
        "right-end"
      ],
      "description": "Tooltip 的出现位置",
      "default": "top | top-start | top-end | bottom | bottom-start | bottom-end | left | left-start | left-end | right | right-start | right-end",
      "type": "String"
    },
    "value": {
      "value": "",
      "description": "状态是否可见",
      "default": "—",
      "type": "Boolean"
    },
    "v-model": {
      "value": "",
      "description": "状态是否可见",
      "default": "—",
      "type": "Boolean",
      "required":true
    },
    "disabled": {
      "value": "",
      "description": "Tooltip 是否可用",
      "default": "—",
      "type": "Boolean"
    },
    "offset": {
      "value": "",
      "description": "出现位置的偏移量",
      "default": "—",
      "type": "Number"
    },
    "transition": {
      "value": "",
      "description": "定义渐变动画",
      "default": "—",
      "type": "String"
    },
    "visible-arrow": {
      "value": "",
      "description": "是否显示 Tooltip 箭头，更多参数可见[Vue-popper](https://github.com/element-component/vue-popper)",
      "default": "—",
      "type": "Boolean"
    },
    ":popper-options": {
      "value": "",
      "description": "[popper.js](https://popper.js.org/documentation.html) 的参数",
      "default": "{ boundariesElement: 'body', gpuAcceleration: false }",
      "type": "Object"
    },
    "open-delay": {
      "value": "",
      "description": "延迟出现，单位毫秒",
      "default": "—",
      "type": "Number"
    },
    "manual": {
      "value": "",
      "description": "手动控制模式，设置为 true 后，mouseenter 和 mouseleave 事件将不会生效",
      "default": "—",
      "type": "Boolean"
    },
    "popper-class": {
      "value": "",
      "description": "为 Tooltip 的 popper 添加类名",
      "default": "—",
      "type": "String"
    },
    "enterable": {
      "value": "",
      "description": "鼠标是否可进入到 tooltip 中",
      "default": "—",
      "type": "Boolean"
    },
    "hide-after": {
      "value": "",
      "description": "Tooltip 出现后自动隐藏延时，单位毫秒，为 0 则不会自动隐藏",
      "default": "—",
      "type": "number"
    },
    "tabindex": {
      "value": "",
      "description": "Tooltip 组件的 [tabindex](https://developer.mozilla.org/en-US/docs/Web/HTML/Global_attributes/tabindex)",
      "default": "—",
      "type": "number"
    }
  },
  "events": [],
  "link": "https://element.eleme.cn/#/zh-CN/component/tooltip"
}
