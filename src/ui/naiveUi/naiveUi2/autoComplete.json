        "type": "`string`"
      },
      {
        "name": "value",
        "description": "需要唯一",
        "type": "`string`"
      }
    ],
    "AutoCompleteGroupOption": [
      {
        "name": "children",
        "description": "AutoCompleteGroupOption 的 children 项",
        "type": "`Array<string | AutoCompleteOption>`"
      },
      {
        "name": "label",
        "description": "AutoCompleteGroupOption 的名字",
        "type": "`string`"
      },
      {
        "name": "key",
        "description": "AutoCompleteGroupOption 的 key",
        "type": "`string | number`"
      },
      {
        "name": "type",
        "description": "AutoCompleteGroupOption 的类型",
        "type": "`'group'`"
      }
    ]
  },
  "events": [
    {
      "name": "blur",
      "description": "blur 时触发的回调函数",
      "params": "`(event: FocusEvent) => void`"
    },
    {
      "name": "focus",
      "description": "focus 时触发的回调函数",
      "params": "`(event: FocusEvent) => void`"
    },
    {
      "name": "select",
      "description": "select 选中时触发的回调函数",
      "params": "`(value: string) => void`"
    },
    {
      "name": "update:value",
      "description": "可控数据更新时触发的回调函数",
      "params": "`(value: string | null) => void`"
    }
  ],
  "link": "https://www.naiveui.com/zh-CN/dark/components/auto-complete"
}
