{
  "name": "NAutoComplete",
  "props": {
    "blur-after-select": {
      "value": "",
      "description": "选中后是否 blur",
      "default": "false",
      "type": "boolean"
    },
    "clear-after-select": {
      "value": "",
      "description": "选中后是否清空",
      "default": "false",
      "type": "boolean"
    },
    "clearable": {
      "value": "",
      "description": "自动填充是否支持可清除",
      "default": "false",
      "type": "boolean"
    },
    "default-value": {
      "value": "",
      "description": "自动填充的默认值",
      "default": "null",
      "type": "string"
    },
    "disabled": {
      "value": "",
      "description": "自动填充是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "get-show": {
      "value": "",
      "description": "根据输入值在聚焦的状态中决定是否显示菜单",
      "default": "undefined",
      "type": "(value: string) => boolean"
    },
    "input-props": {
      "value": "",
      "description": "自动填充中 input 元素的属性",
      "default": "undefined",
      "type": "HTMLInputAttributes"
    },
    "loading": {
      "value": "",
      "description": "是否展示加载状态",
      "default": "false",
      "type": "boolean"
    },
    ":menu-props": {
      "value": "",
      "description": "菜单的 DOM 属性",
      "default": "undefined",
      "type": "HTMLAttributes"
    },
    ":options": {
      "value": "",
      "description": "自动填充的自定义选项",
      "default": "[]",
      "type": "Array<string | AutoCompleteOption | AutoCompleteGroupOption>"
    },
    "placeholder": {
      "value": "",
      "description": "自动填充的提示信息",
      "default": "'请输入'",
      "type": "string"
    },
    "placement": {
      "value": "",
      "description": "自动填充的弹出位置",
      "default": "'bottom-start'",
      "type": "'top-start' | 'top' | 'top-end' | 'right-start' | 'right' | 'right-end' | 'bottom-start' | 'bottom' | 'bottom-end' | 'left-start' | 'left' | 'left-end'"
    },
    "render-label": {
      "value": "",
      "description": "选项标签渲染函数",
      "default": "undefined",
      "type": "(option: SelectOption | SelectGroupOption, selected: boolean) => VNodeChild"
    },
    "render-option": {
      "value": "",
      "description": "选项的渲染函数",
      "default": "undefined",
      "type": "(info: { node: VNode, option: SelectOption | SelectGroupOption, selected: boolean }) => VNodeChild"
    },
    "size": {
      "value": "",
      "description": "自动填充的尺寸大小",
      "default": "'medium'",
      "type": "'small' | 'medium' | 'large'"
    },
    "to": {
      "value": "",
      "description": "菜单的容器节点，`false` 会待在原地",
      "default": "body",
      "type": "string | HTMLElement | false"
    },
    "status": {
      "value": "",
      "description": "验证状态",
      "default": "undefined",
      "type": "'success' | 'warning' | 'error'"
    },
    "value": {
      "value": "",
      "description": "自动填充的数据用户可控",
      "default": "undefined",
      "type": "string"
    },
    "default": {
      "value": "",
      "description": "自定义输入元素，由用户填充",
      "default": "",
      "type": "自定义输入元素，由用户填充"
    },
    "prefix": {
      "value": "",
      "description": "输入框头部内容",
      "default": "",
      "type": "输入框头部内容"
    },
    "suffix": {
      "value": "",
      "description": "输入框尾部内容",
      "default": "",
      "type": "输入框尾部内容"
    }
  },
  "methods": [
    {
      "name": "blur",
      "description": "Blur input 元素",
      "params": "`() => void`"
    },
    {
      "name": "focus",
      "description": "Focus input 元素",
      "params": "`() => void`"
    }
  ],
  "typeDetail": {
    "AutoCompleteOption": [
      {
        "name": "disabled",
        "description": "是否禁用",
        "type": "`boolean`"
      },
      {
        "name": "label",
