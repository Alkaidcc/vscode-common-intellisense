{
  "name": "NTimePicker",
  "props": {
    "actions": {
      "value": "",
      "description": "Time Picker 中支持的操作",
      "default": "['now', 'confirm']",
      "type": "Array<'now' | 'confirm'> | null"
    },
    "clearable": {
      "value": "",
      "description": "是否可清空",
      "default": "false",
      "type": "boolean"
    },
    "default-value": {
      "value": "",
      "description": "非受控模式下的默认值",
      "default": "null",
      "type": "number | null"
    },
    "default-formatted-value": {
      "value": "",
      "description": "非受控模式下的默认格式化后的值",
      "default": "undefined",
      "type": "string | null"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "format": {
      "value": "",
      "description": "时间格式化字符串，详情见 [format](https://date-fns.org/v2.23.0/docs/format)",
      "default": "'HH:mm:ss'",
      "type": "string"
    },
    "formatted-value": {
      "value": "",
      "description": "格式化后的值",
      "default": "undefined",
      "type": "string | null"
    },
    "hours": {
      "value": "",
      "description": "通过数组指定显示的小时。当值为 `number` 时，将被当做时间步进处理",
      "default": "undefined",
      "type": "number | number[]"
    },
    "minutes": {
      "value": "",
      "description": "通过数组指定显示的分钟。当值为 `number` 时，将被当做时间步进处理",
      "default": "undefined",
      "type": "number | number[]"
    },
    "seconds": {
      "value": "",
      "description": "通过数组指定显示的秒。当值为 `number` 时，将被当做时间步进处理",
      "default": "undefined",
      "type": "number | number[]"
    },
    "input-readonly": {
      "value": "",
      "description": "设置输入框为只读（避免在移动设备上打开虚拟键盘）",
      "default": "false",
      "type": "boolean"
    },
    "is-hour-disabled": {
      "value": "",
      "description": "用于禁用小时的回调函数",
      "default": "() => false",
      "type": "(hour: number) => boolean"
    },
    "is-minute-disabled": {
      "value": "",
      "description": "用于禁用分钟的回调函数，在没有选中值时，`hour` 是 `null`",
      "default": "() => false",
      "type": "(minute: number, hour: number | null) => boolean"
    },
    "is-second-disabled": {
      "value": "",
      "description": "用于禁用秒钟的回调函数，在没有选中值时，`minute` 和 `hour` 是 `null`",
      "default": "() => false",
      "type": "(second: number, minute: number | null, hour: number | null) => boolean"
    },
    "placeholder": {
      "value": "",
      "description": "选择框的占位符",
      "default": "'请选择时间'",
      "type": "string"
    },
    "placement": {
      "value": ["top-start", "top", "top-end", "right-start", "right", "right-end", "bottom-start", "bottom", "bottom-end", "left-start", "left", "left-end"],
      "description": "时间选择器的面板的弹出位置",
      "default": "'bottom-start'",
      "type": "'top-start' | 'top' | 'top-end' | 'right-start' | 'right' | 'right-end' | 'bottom-start' | 'bottom' | 'bottom-end' | 'left-start' | 'left' | 'left-end'"
    },
    "show": {
      "value": "",
      "description": "是否展示面板",
      "default": "undefined",
      "type": "boolean"
    },
    "size": {
      "value": ["small", "medium", "large"],
      "description": "选择框的尺寸",
      "default": "'medium'",
      "type": "'small' | 'medium' | 'large'"
    },
    "status": {
      "value": ["success", "warning", "error"],
      "description": "验证状态",
      "default": "undefined",
      "type": "'success' | 'warning' | 'error'"
    },
    "time-zone": {
      "value": "",
      "description": "格式化值时使用的市区，遵循 [iana time zones](https://www.iana.org/time-zones) 格式。你可以使用 `Intl.supportedValuesOf('timeZone')` 来查看支持的时区",
      "default": "undefined",
      "type": "string"
    },
    "to": {
      "value": "",
      "description": "菜单的容器节点，`false` 会待在原地",
      "default": "body",
      "type": "string | HTMLElement | false"
    },
    "use-12-hours": {
      "value": "",
      "description": "是否使用 12 小时制的面板",
      "default": "false",
      "type": "boolean"
    },
    "value": {
      "value": "",
      "description": "受控模式下的值",
      "default": "undefined",
      "type": "number | null"
    },
    "value-format": {
      "value": "",
      "description": "格式化后值的格式",
      "default": "跟随 format",
      "type": "string"
    }
  },
  "methods": [
    {
      "name": "focus",
      "description": "聚焦",
      "params": "`() => void`"
    },
    {
      "name": "blur",
      "description": "失焦",
      "params": "`() => void`"
    }
  ],
  "typeDetail": {},
  "events": [
    {
      "name": "blur",
      "description": "选择框失去焦点时的回调",
      "params": "`() => void`"
    },
    {
      "name": "clear",
      "description": "清除值时的回调",
      "params": "`() => void`"
    },
    {
      "name": "confirm",
      "description": "点击确认按钮时的回调",
      "params": "`(value: number | null, formattedValue: string | null) => void`"
    },
    {
      "name": "focus",
      "description": "选择框获得焦点时的回调",
      "params": "`() => void`"
    },
    {
      "name": "update:formatted-value",
      "description": "格式化的值发生改变时的回调",
      "params": "`(value: number | null, timestampValue: number | null) => void`"
    },
    {
      "name": "update:show",
      "description": "面板打开、关闭时的回调",
      "params": "`(show: boolean) => void`"
    },
    {
      "name": "update:value",
      "description": "值发生改变时的回调",
      "params": "`(value: number | null, formattedValue: string | null) => void`"
    }
  ],
  "link": "https://www.naiveui.com/zh-CN/dark/components/time-picker"
}
