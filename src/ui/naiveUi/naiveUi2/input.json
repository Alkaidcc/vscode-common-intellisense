{
  "name": "NInput",
  "props": {
    "allow-input": {
      "value": "",
      "description": "校验当前的输入是否合法，如果返回 `false` 输入框便不会响应此次的输入",
      "default": "undefined",
      "type": "(value: string) => false"
    },
    "autofocus": {
      "value": "",
      "description": "是否自动获取焦点",
      "default": "false",
      "type": "boolean"
    },
    "autosize": {
      "value": "",
      "description": "自适应内容高度，只对 `type=\"textarea\"` 有效，可传入对象，如 `{ minRows: 1, maxRows: 3 }`",
      "default": "false",
      "type": "boolean | { minRows?: number, maxRows?: number }"
    },
    "clearable": {
      "value": "",
      "description": "是否可清空",
      "default": "false",
      "type": "boolean"
    },
    "default-value": {
      "value": "",
      "description": "输入框默认值",
      "default": "null",
      "type": "string | [string, string] | null"
    },
    "count-graphemes": {
      "value": "",
      "description": "计算输入的字数。如果设定了，那么原生的 `maxlength` 和 `minlength` 属性将不再被使用",
      "default": "undefined",
      "type": "(value: string) => number"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "input-props": {
      "value": "",
      "description": "Input 组件内部 input 元素的属性，对 `pair` 类型不生效，[在这里查看原生属性](https://developer.mozilla.org/en-US/docs/Web/HTML/Element/input)。注意：input-props 不会覆盖内部 input 元素的已经存在的属性（除了 `type`）",
      "default": "undefined",
      "type": "HTMLInputAttributes"
    },
    "loading": {
      "value": "",
      "description": "是否展示加载图标，设为非 `undefined` 会占据空间",
      "default": "undefined",
      "type": "boolean"
    },
    "maxlength": {
      "value": "",
      "description": "最大输入长度",
      "default": "undefined",
      "type": "number"
    },
    "minlength": {
      "value": "",
      "description": "最小输入长度",
      "default": "undefined",
      "type": "number"
    },
    "pair": {
      "value": "",
      "description": "是否输入成对的值",
      "default": "false",
      "type": "boolean"
    },
    "passively-activated": {
      "value": "",
      "description": "是否被动激活输入框",
      "default": "false",
      "type": "boolean"
    },
    "placeholder": {
      "value": "",
      "description": "文本输入的占位符。如果 `pair` 是 `true`，`placeholder`是一个数组",
      "default": "undefined",
      "type": "string | [string, string]"
    },
    "readonly": {
      "value": "",
      "description": "是否只读",
      "default": "false",
      "type": "boolean"
    },
    "render-count": {
      "value": "",
      "description": "字数统计的渲染函数",
      "default": "undefined",
      "type": "(props: { value: string }) => void"
    },
    "round": {
      "value": "",
      "description": "输入框是否圆角",
      "default": "false",
      "type": "boolean"
    },
    "rows": {
      "value": "",
      "description": "输入框行数，对 type=\"textarea\" 有效",
      "default": "3",
      "type": "number"
    },
    "separator": {
      "value": "",
      "description": "成对输入框中间的分隔符",
      "default": "undefined",
      "type": "string"
    },
    "show-count": {
      "value": "",
      "description": "是否显示字数统计",
      "default": "false",
      "type": "boolean"
    },
    "show-password-on": {
      "value": ["click", "mousedown"],
      "description": "显示密码的时机",
      "default": "undefined",
      "type": "'click' | 'mousedown'"
    },
    "size": {
      "value": ["tiny", "small", "medium", "large"],
      "description": "输入框尺寸",
      "default": "'medium'",
      "type": "'tiny' | 'small' | 'medium' | 'large'"
    },
    "status": {
      "value": ["success", "warning", "error"],
      "description": "验证状态",
      "default": "undefined",
      "type": "'success' | 'warning' | 'error'"
    },
    "type": {
      "value": ["text", "password", "textarea"],
      "description": "输入框类型",
      "default": "'text'",
      "type": "'text' | 'password' | 'textarea'"
    },
    "value": {
      "value": "",
      "description": "文本输入的值。如果 `pair` 是 `true`，`value` 是一个数组",
      "default": "undefined",
      "type": "string | [string, string] | null"
    },
    "blur": {
      "value": "",
      "description": "Blur input 元素",
      "default": "",
      "type": "() => void"
    },
    "focus": {
      "value": "",
      "description": "Focus input 元素",
      "default": "",
      "type": "() => void"
    },
    "select": {
      "value": "",
      "description": "Select input 元素",
      "default": "",
      "type": "() => void"
    },
    "scrollTo": {
      "value": "",
      "description": "滚动到",
      "default": "2.32.0",
      "type": "(options: { left?: number, top?: number, behavior?: 'auto' | 'smooth' }) => void"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "blur",
      "description": "输入框失去焦点时触发",
      "params": "`() => void`"
    },
    {
      "name": "change",
      "description": "原生 change 事件触发时触发",
      "params": "`(value: string | [string, string]) => void`"
    },
    {
      "name": "clear",
      "description": "输入框点击清空按钮时触发",
      "params": "`() => void`"
    },
    {
      "name": "focus",
      "description": "输入框获得焦点时触发",
      "params": "`() => void`"
    },
    {
      "name": "input",
      "description": "输入框在用户输入时触发",
      "params": "`(value: string | [string, string]) => void`"
    },
    {
      "name": "update:value",
      "description": "输入框值 change 时触发",
      "params": "`(value: string | [string, string]) => void`"
    }
  ],
  "link": "https://www.naiveui.com/zh-CN/dark/components/input"
}
