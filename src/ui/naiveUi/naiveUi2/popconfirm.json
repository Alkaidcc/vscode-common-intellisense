{
  "name": "NPopconfirm",
  "props": {
    ":negative-button-props": {
      "value": "",
      "description": "取消按钮的属性",
      "default": "undefined",
      "type": "ButtonProps"
    },
    "negative-text": {
      "value": "",
      "description": "取消按钮文字",
      "default": "",
      "type": "string | null"
    },
    "positive-button-props": {
      "value": "",
      "description": "确定按钮的属性",
      "default": "undefined",
      "type": "ButtonProps"
    },
    "positive-text": {
      "value": "",
      "description": "确定按钮文字",
      "default": "",
      "type": "string | null"
    },
    "show-icon": {
      "value": "",
      "description": "是否显示图标",
      "default": "true",
      "type": "boolean"
    },
    "animated": {
      "value": "",
      "description": "弹出弹窗时使用动画",
      "default": "true",
      "type": "boolean"
    },
    "arrow-style": {
      "value": "",
      "description": "popover 的箭头样式",
      "default": "undefined",
      "type": "Object"
    },
    "arrow-point-to-center": {
      "value": "",
      "description": "箭头是否指向触发元素中心",
      "default": "false",
      "type": "boolean"
    },
    "content-style": {
      "value": "",
      "description": "popover 内容的样式",
      "default": "undefined",
      "type": "Object | string"
    },
    "delay": {
      "value": "",
      "description": "悬浮触发弹出信息的延迟",
      "default": "100",
      "type": "number"
    },
    "disabled": {
      "value": "",
      "description": "是否不能激活弹出信息",
      "default": "false",
      "type": "boolean"
    },
    "display-directive": {
      "value": ["if", "show"],
