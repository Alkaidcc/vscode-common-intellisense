{
  "name": "NMenu",
  "props": {
    "accordion": {
      "value": "",
      "description": "是否使用手风琴模式",
      "default": "false",
      "type": "boolean"
    },
    "children-field": {
      "value": "",
      "description": "children 的字段名",
      "default": "'children'",
      "type": "string"
    },
    "collapsed-icon-size": {
      "value": "",
      "description": "菜单折叠时图标的大小，如果未设定则使用 `icon-size` 代替",
      "default": "24",
      "type": "number"
    },
    "collapsed-width": {
      "value": "",
      "description": "折叠后菜单的宽度",
      "default": "48",
      "type": "number"
    },
    "collapsed": {
      "value": "",
      "description": "菜单是否折叠，值在菜单为垂直时有用",
      "default": "false",
      "type": "boolean"
    },
    "default-expand-all": {
      "value": "",
      "description": "是否展开全部菜单，`options` 为异步获取时，`watch-props` 需要设置为 `['defaultExpandedKeys']` 才会生效",
      "default": "false",
      "type": "boolean"
    },
    "default-expanded-keys": {
      "value": "",
      "description": "在非受控状态下默认展开的子菜单标识符数组",
      "default": "[]",
      "type": "Array<string>"
    },
    "default-value": {
      "value": "",
      "description": "非受控模式下的默认值",
      "default": "null",
      "type": "string | null"
    },
    "disabled-field": {
      "value": "",
      "description": "disabled 的字段名",
      "default": "'disabled'",
      "type": "string"
    },
    "dropdown-placement": {
      "value": "",
      "description": "仅在 `mode='horizontal'` 模式下生效",
      "default": "'top'",
      "type": "'top-start' | 'top' | 'top-end' | 'right-start' | 'right' | 'right-end' | 'bottom-start' | 'bottom' | 'bottom-end' | 'left-start' | 'left' | 'left-end' | "
    },
    "dropdown-props": {
      "value": "",
      "description": "菜单折叠或 `mode='horizontal'` 模式时 Dropdown 的 props，请参考 [Dropdown Props](dropdown#Dropdown-Props)",
      "default": "undefined",
      "type": "DropdownProps"
    },
    "expanded-keys": {
      "value": "",
      "description": "展开的子菜单标识符数组，如果设定了，菜单的展开将会进入受控状态，`default-expanded-keys` 不会生效",
      "default": "undefined",
      "type": "Array<string>"
    },
    "expand-icon": {
      "value": "",
      "description": "批量处理菜单展开图标的渲染",
      "default": "undefined",
      "type": "(option: MenuOption) => VNodeChild"
    },
    "icon-size": {
      "value": "",
      "description": "菜单未折叠时图标的大小",
      "default": "20",
      "type": "number"
    },
    "indent": {
      "value": "",
      "description": "菜单每级的缩进",
      "default": "32",
      "type": "number"
    },
    "inverted": {
      "value": "",
      "description": "使用反转样式",
      "default": "false",
      "type": "boolean"
    },
    "key-field": {
      "value": "",
      "description": "key 的字段名",
      "default": "'key'",
      "type": "string"
    },
    "label-field": {
      "value": "",
      "description": "label 的字段名",
      "default": "'label'",
      "type": "string"
    },
    "options": {
      "value": "",
      "description": "菜单的数据",
      "default": "[]",
      "type": "Array<MenuOption | MenuDividerOption | MenuGroupOption>"
    },
    "node-props": {
      "value": "",
      "description": "节点的 DOM 属性生成函数",
      "default": "undefined",
      "type": "(option: MenuOption | MenuGroupOption) => object"
    },
    "mode": {
      "value": "",
      "description": "菜单的布局方式",
      "default": "'vertical'",
      "type": "'vertical' | 'horizontal'"
    },
    "render-extra": {
      "value": "",
      "description": "批量处理菜单额外部分渲染",
      "default": "undefined",
      "type": "(option: MenuOption | MenuGroupOption) => VNodeChild"
    },
    "render-icon": {
      "value": "",
      "description": "批量处理菜单图标渲染",
      "default": "undefined",
      "type": "(option: MenuOption) => VNodeChild"
    },
    "render-label": {
      "value": "",
      "description": "批量处理菜单标签渲染",
      "default": "undefined",
      "type": "(option: MenuOption | MenuGroupOption) => VNodeChild"
    },
    "root-indent": {
      "value": "",
      "description": "菜单第一级的缩进，如果没有设定，使用 `indent` 代替",
      "default": "32",
      "type": "number"
    },
    "value": {
      "value": "",
      "description": "菜单当前的选中值",
      "default": "undefined",
      "type": "string | null"
    },
    "watch-props": {
      "value": "",
      "description": "需要检测变更的默认属性，检测后组件状态会更新。注意：`watch-props` 本身不是响应式的",
      "default": "undefined",
      "type": "Array<'defaultValue' | 'defaultExpandedKeys'>"
    }
  },
  "methods": [
    {
      "name": "showOption",
      "description": "展开菜单，确保设定的元素被显示，如果不传入 `key` 会当前选中元素",
      "params": "`(key?: string | number) => void`"
    }
  ],
  "typeDetail": {
    "MenuOption": [
      {
        "name": "children?",
        "description": "子选项",
        "type": "`Array<MenuOption | MenuGroupOption>`"
      },
      {
        "name": "disabled?",
        "description": "是否禁用菜单项",
        "type": "`boolean`"
      },
      {
        "name": "extra?",
        "description": "菜单项的额外部分",
        "type": "`string | (() => VNodeChild)`"
      },
      {
        "name": "icon?",
        "description": "菜单项的图标",
        "type": "`() => VNode`"
      },
      {
        "name": "key",
        "description": "菜单项的标识符",
        "type": "`string`"
      },
      {
        "name": "label",
        "description": "菜单项的内容",
        "type": "`string | (() => VNodeChild)`"
      },
      {
        "name": "show?",
        "description": "是否显示菜单项",
        "type": "`boolean`"
      }
    ],
    "MenuGroupOption": [
      {
        "name": "children",
        "description": "子菜单，**必填！**",
        "type": "`Array<MenuOption | MenuGroupOption>`"
      },
      {
        "name": "key",
        "description": "菜单项的标识符",
        "type": "`string`"
      },
      {
        "name": "label",
        "description": "菜单项的内容",
        "type": "`string | (() => VNodeChild)`"
      },
      {
        "name": "show?",
        "description": "是否显示菜单项",
        "type": "`boolean`"
      },
      {
        "name": "type",
        "description": "菜单项的类型，**必填！**",
        "type": "`'group'`"
      }
    ],
    "MenuDividerOption": [
      {
        "name": "key",
        "description": "菜单项的标识符",
        "type": "`string`"
      },
      {
        "name": "props",
        "description": "分割线的属性",
        "type": "`HTMLAttributes`"
      },
      {
        "name": "show?",
        "description": "是否显示菜单项",
        "type": "`boolean`"
      },
      {
        "name": "type",
        "description": "菜单项的类型，**必填！**",
        "type": "`'divider'`"
      }
    ]
  },
  "events": [
    {
      "name": "update:expanded-keys",
      "description": "`keys` 是展开菜单项的 `key` 的数组",
      "params": "`(keys: string[]) => void`"
    },
    {
      "name": "update:value",
      "description": "选中菜单的回调，`key` 是选中菜单项的 `key`，`item` 是菜单项原始数据",
      "params": "`(key: string, item: MenuOption) => void`"
    }
  ],
  "link": "https://www.naiveui.com/zh-CN/dark/components/menu"
}
