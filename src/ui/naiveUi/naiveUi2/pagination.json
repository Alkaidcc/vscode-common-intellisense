{
  "name": "NPagination",
  "props": {
    "default-page": {
      "value": "",
      "description": "非受控模式下的当前页",
      "default": "1",
      "type": "number"
    },
    "default-page-size": {
      "value": "",
      "description": "非受控模式下的分页大小",
      "default": "10",
      "type": "number"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "display-order": {
      "value": "",
      "description": "不同部分的展示顺序",
      "default": "[pages, size-picker, quick-jumper]",
      "type": "Array<pages | size-picker | quick-jumper>"
    },
    "goto": {
      "value": "",
      "description": "渲染快速跳转的文本内容",
      "default": "undefined",
      "type": "() => VNodeChild"
    },
    "item-count": {
      "value": "",
      "description": "总条数",
      "default": "undefined",
      "type": "number"
    },
    "next": {
      "value": "",
      "description": "下一页",
      "default": "undefined",
      "type": "(info: PaginationInfo) => VNodeChild"
    },
    "prev": {
      "value": "",
      "description": "上一页",
      "default": "undefined",
      "type": "(info: PaginationInfo) => VNodeChild"
    },
    "label": {
      "value": "",
      "description": "每一项的内容",
      "default": "undefined",
      "type": "PaginationRenderLabel"
    },
    "page-count": {
      "value": "",
      "description": "总页数",
      "default": "1",
      "type": "number"
    },
    "page-sizes": {
      "value": "",
      "description": "每页条数， 可自定义",
      "default": "[10]",
      "type": "Array<number | PaginationSizeOption>"
    },
    "page-size": {
      "value": "",
      "description": "受控模式下的分页大小",
      "default": "undefined",
      "type": "number"
    },
    "page-slot": {
      "value": "",
      "description": "页码显示的个数",
      "default": "9",
      "type": "number"
    },
    "page": {
      "value": "",
      "description": "受控模式下的当前页",
      "default": "undefined",
      "type": "number"
    },
    "prefix": {
      "value": "",
      "description": "分页前缀",
      "default": "undefined",
      "type": "(info: PaginationInfo) => VNodeChild"
    },
    "select-props": {
      "value": "",
      "description": "分页大小选择器的属性",
      "default": "undefined",
      "type": "SelectProps"
    },
    "show-quick-jumper": {
      "value": "",
      "description": "是否显示快速跳转",
      "default": "false",
      "type": "boolean"
    },
    "size": {
      "value": "",
      "description": "分页按钮的大小",
      "default": "medium",
      "type": "small | medium | large"
    },
    "simple": {
      "value": "",
      "description": "是否显示为简单分页",
      "default": "false",
      "type": "boolean"
    },
    "suffix": {
      "value": "",
      "description": "分页后缀",
      "default": "undefined",
      "type": "(info: PaginationInfo) => VNodeChild"
    },
    "show-size-picker": {
      "value": "",
      "description": "是否显示每页条数的选择器",
      "default": "false",
      "type": "boolean"
    },
    "to": {
      "value": "",
      "description": "弹出菜单的容器节点，`false` 会待在原地",
      "default": "body",
      "type": "string | HTMLElement | false"
    }
  },
  "methods": [],
  "typeDetail": {
    "PaginationRenderLabel": [
      {
        "name": "info",
        "description": "_",
        "type": "VNodeChild"
      }
    ],
    "VNodeChild": [
      {
        "name": "type",
        "description": "_",
        "type": "string"
      },
      {
        "name": "node",
        "description": "_",
        "type": "VNode"
      },
      {
        "name": "active",
        "description": "_",
        "type": "boolean"
      }
    ],
    "PaginationInfo": [
      {
        "name": "startIndex",
        "description": "_",
        "type": "number"
      },
      {
        "name": "endIndex",
        "description": "_",
        "type": "number"
      },
      {
        "name": "page",
        "description": "_",
        "type": "number"
      },
      {
        "name": "pageSize",
        "description": "_",
        "type": "number"
      },
      {
        "name": "pageCount",
        "description": "_",
        "type": "number"
      },
      {
        "name": "itemCount",
        "description": "_",
        "type": "number | undefined"
      }
    ]
  },
  "events": [
    {
      "name": "update:page",
      "description": "当前页发生改变时的回调函数",
      "params": "(page: number) => void"
    },
    {
      "name": "update:page-size",
      "description": "当前分页大小发生改变时的回调函数",
      "params": "(pageSize: number) => void"
    }
  ],
  "link": "https://www.naiveui.com/zh-CN/os-theme/components/pagination"
