{
  "name": "NModal",
  "props": {
    "auto-focus": {
      "value": "",
      "description": "是否自动聚焦 Modal 第一个可聚焦的元素",
      "default": "true",
      "type": "boolean"
    },
    "block-scroll": {
      "value": "",
      "description": "是否在打开时禁用 body 滚动",
      "default": "true",
      "type": "boolean"
    },
    "close-on-esc": {
      "value": "",
      "description": "是否在摁下 Esc 键的时候关闭 Modal",
      "default": "true",
      "type": "boolean"
    },
    "display-directive": {
      "value": "",
      "description": "使用何种指令控制模态框主体的条件渲染",
      "default": "'if'",
      "type": "'if' | 'show'"
    },
    "mask-closable": {
      "value": "",
      "description": "点击遮罩时是否发出 `update:show` 事件",
      "default": "true",
      "type": "boolean"
    },
    "preset": {
      "value": "",
      "description": "模态框使用何种预设",
      "default": "undefined",
      "type": "'dialog' | 'card'"
    },
    "show": {
      "value": "",
      "description": "是否展示 Modal",
      "default": "false",
      "type": "boolean"
    },
    "to": {
      "value": "",
      "description": "Modal 的挂载位置",
      "default": "body",
      "type": "string | HTMLElement"
    },
    "transform-origin": {
      "value": "",
      "description": "模态框动画出现的位置",
      "default": "'mouse'",
      "type": "'mouse' | 'center'"
    },
    "trap-focus": {
      "value": "",
      "description": "是否将焦点锁定在 Modal 内部",
      "default": "true",
      "type": "boolean"
    },
    "z-index": {
      "value": "",
      "description": "Modal 的 z-index",
      "default": "undefined",
      "type": "number"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "after-enter",
      "description": "Modal 出现后的回调",
      "params": "`() => void`"
    },
    {
      "name": "after-leave",
      "description": "Modal 关闭后的回调",
      "params": "`() => void`"
    },
    {
      "name": "esc",
      "description": "焦点在 Modal 内部时按下 Esc 键的回调",
      "params": "`() => void`"
    },
    {
      "name": "mask-click",
      "description": "点击遮罩时的回调",
      "params": "`() => void`"
    },
    {
      "name": "update:show",
      "description": "模态框更新是否展示状态的回调",
      "params": "`(value: boolean) => void`"
    }
  ],
  "link": "https://www.naiveui.com/zh-CN/dark/components/modal"
}
