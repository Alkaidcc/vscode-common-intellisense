{
  "name": "NForm",
  "props": {
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "inline": {
      "value": "",
      "description": "是否展示为行内表单",
      "default": "false",
      "type": "boolean"
    },
    "label-width": {
      "value": "",
      "description": "标签的宽度，在 `label-placement` 是 left 的时候可能会有用，auto 意味着 label width 会被自动调整",
      "default": "undefined",
      "type": "number | string | auto"
    },
    "label-align": {
      "value": ["left", "right"],
      "description": "标签的文本对齐方式",
      "default": "-",
      "type": "left | right"
    },
    "label-placement": {
      "value": ["left", "top"],
      "description": "标签显示的位置",
      "default": "top",
      "type": "left | top"
    },
    "model": {
      "value": "",
      "description": "获取表项中收集到的值的对象",
      "default": "{}",
      "type": "Object"
    },
    "rules": {
      "value": "",
      "description": "验证表项的规则",
      "default": "{}",
      "type": "type FormRules = { [itemValidatePath: string]: FormItemRule | Array<FormItemRule> | FormRules }"
    },
    "show-feedback": {
      "value": "",
      "description": "是否展示校验反馈",
      "default": "true",
      "type": "boolean"
    },
    "show-label": {
      "value": "",
      "description": "是否展示标签",
      "default": "true",
      "type": "boolean"
    },
    "show-require-mark": {
      "value": "",
      "description": "是否展示必填的星号",
      "default": "false",
      "type": "boolean"
    },
    "require-mark-placement": {
      "value": ["left", "right", "right-hanging"],
      "description": "必填星号的位置",
      "default": "right",
      "type": "left | right | right-hanging"
    },
    "size": {
      "value": ["small", "medium", "large"],
      "description": "尺寸",
      "default": "medium",
      "type": "small | medium | large"
    },
    "validate-messages": {
      "value": "",
      "description": "`async-validator` 的默认验证信息",
      "default": "undefined",
      "type": "FormValidateMessages"
    }
  },
  "methods": [
    {
      "name": "validate",
      "description": "验证表单，Promise rejection 的返回值类型是 `Array<FormValidationError>`",
      "params": "(validateCallback?: (errors?: Array<FormValidationError>) => void, shouldRuleBeApplied?: FormItemRule => boolean) => Promise<void>"
    },
    {
      "name": "restoreValidation",
      "description": "还原到未校验的状态",
      "params": "() => void"
    }
  ],
  "typeDetail": {
    "FormItemRule": [
      {
        "name": "asyncValidator",
        "description": "异步校验，支持定义回调函数",
        "type": "(rule: FormItemRule, value: any, callback: (error?: Error) => void) => void"
      },
      {
        "name": "message",
        "description": "校验失败时展示的信息",
        "type": "string"
      },
      {
        "name": "renderMessage",
        "description": "信息的渲染函数",
        "type": "() => VNodeChild"
      },
      {
        "name": "required",
        "description": "是否必填",
        "type": "boolean"
      },
      {
        "name": "trigger",
        "description": "触发方式",
        "type": "string | Array<string>"
      },
      {
        "name": "validator",
        "description": "校验规则",
        "type": "(rule: FormItemRule, value: any) => boolean | Error"
      }
    ]
  },
  "events": [],
  "link": "https://www.naiveui.com/zh-CN/dark/components/form"
}
