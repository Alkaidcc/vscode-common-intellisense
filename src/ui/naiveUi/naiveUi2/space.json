{
  "name": "NSpace",
  "props": {
    "align": {
      "value": ["start", "end", "center", "baseline", "stretch"],
      "description": "垂直排列方式",
      "default": "undefined",
      "type": "start | end | center | baseline | stretch"
    },
    "inline": {
      "value": "",
      "description": "是否为行内元素",
      "default": "false",
      "type": "boolean"
    },
    "wrap-item": {
      "value": "",
      "description": "是否存在包裹子元素的容器，`false` 值只会对支持 flex gap 的浏览器生效",
      "default": "true",
      "type": "boolean"
    },
    "item-style": {
      "value": "",
      "description": "节点样式，当 `wrap-item` 为 `true` 时有效",
      "default": "undefined",
      "type": "string | object"
    },
    "justify": {
      "value": ["start", "end", "center", "space-around", "space-between", "space-evenly"],
      "description": "水平排列方式",
      "default": "start",
      "type": "start | end | center | space-around | space-between | space-evenly"
    },
    "size": {
      "value": ["small", "medium", "large"],
      "description": "为数字时，是水平和垂直间距；为数组时，是 [水平间距, 垂直间距]",
      "default": "medium",
      "type": "small | medium | large | number | [number, number]"
    },
    "vertical": {
      "value": "",
      "description": "是否垂直布局",
      "default": "false",
      "type": "boolean"
    },
    "wrap": {
      "value": "",
      "description": "是否超出换行",
      "default": "true",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [],
