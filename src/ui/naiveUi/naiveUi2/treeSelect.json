{
  "name": "NTreeSelect",
  "props": {
    "allow-checking-not-loaded": {
      "value": "",
      "description": "是否允许级联勾选还没有完全加载的节点。如果你要用这个属性，请记住 `value` 可能是不完整的，并且请注意勾选行为和后端计算逻辑的一致性，尤其是有禁用节点的情况下",
      "default": "false",
      "type": "boolean"
    },
    "cascade": {
      "value": "",
      "description": "使用 checkbox 进行多选时是否级联",
      "default": "false",
      "type": "boolean"
    },
    "checkable": {
      "value": "",
      "description": "是否使用 checkbox 进行选择",
      "default": "false",
      "type": "boolean"
    },
    "check-strategy": {
      "value": "",
      "description": "设置勾选策略来指定显示的勾选节点，`all` 表示显示全部选中节点；`parent` 表示只显示父节点（当父节点下所有子节点都选中时）；`child` 表示只显示子节点",
      "default": "all",
      "type": "string"
    },
    "children-field": {
      "value": "",
      "description": "替代 `TreeSelectOption` 中的 children 字段名",
      "default": "children",
      "type": "string"
    },
    "clearable": {
      "value": "",
      "description": "是否可清除",
      "default": "false",
      "type": "boolean"
    },
    "clear-filter-after-select": {
      "value": "",
      "description": "是否在可过滤和多选的情况下选中一个选项后保留当前的搜索关键词",
      "default": "true",
      "type": "boolean"
    },
    "consistent-menu-width": {
      "value": "",
      "description": "是否使菜单宽度和输入框一致，打开会禁用虚拟滚动",
      "default": "true",
      "type": "boolean"
    },
    "default-value": {
      "value": "",
      "description": "默认选中的 key",
      "default": "null",
      "type": "string | number | Array<string | number> | null"
    },
    "default-expand-all": {
      "value": "",
      "description": "默认展开全部",
      "default": "false",
      "type": "boolean"
    },
    "default-expanded-keys": {
      "value": "",
      "description": "默认展开节点的 key",
      "default": "[]",
      "type": "Array<string | number>"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "expanded-keys": {
      "value": "",
      "description": "展开节点的 key",
      "default": "undefined",
      "type": "Array<string | number>"
    },
    "indeterminate-keys": {
      "value": "",
      "description": "部分选中选项的 key",
      "default": "undefined",
      "type": "string | number"
    },
    "filterable": {
      "value": "",
      "description": "是否可过滤",
      "default": "false",
      "type": "boolean"
    },
    "filter": {
      "value": "",
      "description": "过滤器函数",
      "default": "-",
      "type": "(pattern: string, option: TreeSelectOption) => boolean"
    },
    "key-field": {
      "value": "",
      "description": "替代 `TreeSelectOption` 中的 key 字段名",
      "default": "key",
      "type": "string"
    },
    "label-field": {
      "value": "",
      "description": "替代 `TreeSelectOption` 中的 label 字段名",
      "default": "label",
      "type": "string"
    },
    "disabled-field": {
      "value": "",
      "description": "替代 `TreeSelectOption` 中的 disabled 字段名",
      "default": "disabled",
      "type": "string"
    },
    "loading": {
      "value": "",
      "description": "是否加载中",
      "default": "false",
      "type": "boolean"
    },
    "max-tag-count": {
      "value": "",
      "description": "多选时最多直接显示多少选项，设为 responsive 会保证最多一行",
      "default": "undefined",
      "type": "number | responsive"
    },
    "menu-props": {
      "value": "",
      "description": "菜单的 DOM 属性",
      "default": "undefined",
      "type": "HTMLAttributes"
    },
    "multiple": {
      "value": "",
      "description": "是否支持多选",
      "default": "false",
      "type": "boolean"
    },
    "node-props": {
      "value": "",
      "description": "节点的 HTML 属性",
      "default": "undefined",
      "type": "(info: { option: TreeSelectOption }) => HTMLAttributes"
    },
    "options": {
      "value": "",
      "description": "选项",
      "default": "[]",
      "type": "TreeSelectOption[]"
    },
    "placeholder": {
      "value": "",
      "description": "占位信息",
      "default": "",
      "type": "string"
    },
    "placement": {
      "value": "",
      "description": "选择器的弹出位置.",
      "default": "bottom-start",
      "type": "top-start | top | top-end | right-start | right | right-end | bottom-start | bottom | bottom-end | left-start | left | left-end"
    },
    "render-label": {
      "value": "",
      "description": "节点内容的渲染函数",
      "default": "undefined",
      "type": "(info: { option: TreeSelectOption, checked: boolean, selected: boolean }) => VNodeChild"
    },
    "render-prefix": {
      "value": "",
      "description": "节点前缀的渲染函数",
      "default": "undefined",
      "type": "(info: { option: TreeSelectOption, checked: boolean, selected: boolean }) => VNodeChild"
    },
    "render-suffix": {
      "value": "",
      "description": "节点后缀的渲染函数",
      "default": "undefined",
      "type": "(info: { option: TreeSelectOption, checked: boolean, selected: boolean }) => VNodeChild"
    },
    "render-switcher-icon": {
      "value": "",
      "description": "节点展开开关的渲染函数",
      "default": "undefined",
      "type": "() => VNodeChild"
    },
    "render-tag": {
      "value": "",
      "description": "控制标签的渲染",
      "default": "undefined",
      "type": "(props: { option: TreeSelectOption, handleClose: () => void }) => VNodeChild"
    },
    "separator": {
      "value": "",
      "description": "数据分隔符",
      "default": " / ",
      "type": "string"
    },
    "show-path": {
      "value": "",
      "description": "是否在选择器中显示选项路径",
      "default": "false",
      "type": "boolean"
    },
    "size": {
      "value": ["small", "medium", "large"],
      "description": "组件尺寸",
      "default": "medium",
      "type": "small | medium | large"
    },
    "status": {
      "value": ["success", "warning", "error"],
      "description": "验证状态",
      "default": "undefined",
      "type": "success | warning | error"
    },
    "to": {
      "value": "",
      "description": "菜单的容器节点，`false` 会待在原地",
      "default": "body",
      "type": "string | HTMLElement | false"
    },
    "value": {
      "value": "",
      "description": "选中的 key",
      "default": "undefined",
      "type": "string | number | Array<string | number> | null>"
    },
    "virtual-scroll": {
      "value": "",
      "description": "是否开启虚拟滚动",
      "default": "true",
      "type": "boolean"
    }
  },
  "methods": [
    {
      "name": "blur",
      "description": "失焦",
      "params": "() => void"
    },
    {
      "name": "blurInput",
      "description": "输入失焦",
      "params": "() => void"
    },
    {
      "name": "focus",
      "description": "聚焦",
      "params": "() => void"
    },
    {
      "name": "focusInput",
      "description": "输入聚焦",
      "params": "() => void"
    },
    {
      "name": "getCheckedData",
      "description": "获取选中的数据",
      "params": "() => { keys: Array<string | number>, options: Array<TreeOption | null> }"
    },
    {
      "name": "getIndeterminateData",
      "description": "获取半选的数据",
      "params": "() => { keys: Array<string | number>, options: Array<TreeOption | null> }"
    }
  ],
  "typeDetail": {
    "TreeSelectOption": [
      {
        "name": "key",
        "description": "选项的 key，需要唯一，可使用 `key-field` 修改字段名",
        "type": "string | number"
      },
      {
        "name": "label",
        "description": "选项的显示内容，可使用 `label-field` 修改字段名",
        "type": "string"
      },
      {
        "name": "children?",
        "description": "节点的子选项",
        "type": "TreeSelectOption[]"
      },
      {
        "name": "disabled?",
        "description": "是否禁用选项",
        "type": "boolean"
      },
      {
        "name": "isLeaf?",
        "description": "节点是否是叶节点，在异步展开状态下是必须的",
        "type": "boolean"
      }
    ]
  },
  "events": [
    {
      "name": "blur",
      "description": "Blur 时的回调",
      "params": "(e: FocusEvent) => void"
    },
    {
      "name": "focus",
      "description": "Focus 时的回调",
      "params": "(e: FocusEvent) => void"
    },
    {
      "name": "load",
      "description": "异步加载数据的回调函数",
      "params": "(node: TreeSelectOption) => Promise<void>"
    },
    {
      "name": "update:expanded-keys",
      "description": "展开节点更新的回调",
      "params": "(value: Array<string | number>, meta: { node: TreeOption | null, action: expand | collapse | filter }) => void"
    },
    {
      "name": "update:indeterminate-keys",
      "description": "节点部分勾选项发生变化时的回调函数",
      "params": "(keys: Array<string | number>) => void"
    },
    {
      "name": "update:value",
      "description": "更新值的回调",
      "params": "(value: string | number | Array<string | number> | null, option: TreeSelectOption | null | Array<TreeSelectOption | null>, meta: { node: TreeOption | null, action: select | unselect | delete | clear }) => void"
    }
  ],
  "link": "https://www.naiveui.com/zh-CN/dark/components/tree-select"
}
