{
  "name": "ElRow",
  "props": {
    "gutter": {
      "value": "",
      "description": "栅格间隔",
      "type": "number",
      "default": 0
    },
    "type": {
      "value": "",
      "description": "布局模式，可选flex，现代浏览器下有效",
      "default": "",
      "type": "string"
    },
    "justify": {
      "value": [
        "start",
        "end",
        "center",
        "space-around",
        "space-between"
      ],
      "description": "flex布局下的水平排列方式",
      "default": "start",
      "type": "string"
    },
    "align": {
      "value": [
        "top",
        "middle",
        "bottom"
      ],
      "description": "flex布局下的垂直排列方式",
      "default": "",
      "type": "string"
    },
    "tag": {
      "value": "",
      "description": "自定义元素标签",
      "default": "div",
      "type": "string"
    }
  }
}
