{
  "name": "ElPagination",
  "props": {
    "small": {
      "value": "",
      "description": "是否使用小型分页样式",
      "default": "false",
      "type": "boolean"
    },
    "background": {
      "value": "",
      "description": "是否为分页按钮添加背景色",
      "default": "false",
      "type": "boolean"
    },
    "page-size": {
      "value": "",
      "description": "每页显示条目个数，支持 .sync 修饰符",
      "default": "—",
      "type": "number"
    },
    "total": {
      "value": "",
      "description": "总条目数",
      "default": "—",
      "type": "number"
    },
    "page-count": {
      "value": "",
      "description": "总页数，total 和 page-count 设置任意一个就可以达到显示页码的功能；如果要支持 page-sizes 的更改，则需要使用 total 属性",
      "default": "—",
      "type": "Number"
    },
    "pager-count": {
      "value": "",
      "description": "页码按钮的数量，当总页数超过该值时会折叠",
      "default": "大于等于 5 且小于等于 21 的奇数",
      "type": "number"
    },
    "current-page": {
      "value": "",
      "description": "当前页数，支持 .sync 修饰符",
      "default": "—",
      "type": "number"
    },
    "layout": {
      "value": "",
      "description": "组件布局，子组件名用逗号分隔",
      "default": "sizes, prev, pager, next, jumper, `->`, total, slot",
      "type": "String"
    },
    "page-sizes": {
      "value": "",
      "description": "每页显示个数选择器的选项设置",
      "default": "—",
      "type": "number[]"
    },
    "popper-class": {
      "value": "",
      "description": "每页显示个数选择器的下拉框类名",
      "default": "—",
      "type": "string"
    },
    "prev-text": {
      "value": "",
      "description": "替代图标显示的上一页文字",
      "default": "—",
      "type": "string"
    },
    "next-text": {
      "value": "",
      "description": "替代图标显示的下一页文字",
      "default": "—",
      "type": "string"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "hide-on-single-page": {
      "value": "",
      "description": "只有一页时是否隐藏",
      "default": "false",
      "type": "boolean"
    }
  },
  "events": [
    {
      "name": "size-change",
      "description": "pageSize 改变时会触发",
      "params": "每页条数"
    },
    {
      "name": "current-change",
      "description": "currentPage 改变时会触发",
      "params": "当前页"
    },
    {
      "name": "prev-click",
      "description": "用户点击上一页按钮改变当前页后触发",
      "params": "当前页"
    },
    {
      "name": "next-click",
      "description": "用户点击下一页按钮改变当前页后触发",
      "params": "当前页"
    }
  ],
  "link": "https://element.eleme.cn/#/zh-CN/component/pagination"
}
