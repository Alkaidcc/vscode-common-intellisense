{
  "name": "ElDialog",
  "props": {
    "visible": {
      "value": "",
      "description": "是否显示 Dialog，支持 .sync 修饰符",
      "default": "false",
      "type": "boolean"
    },
    "title": {
      "value": "",
      "description": "Dialog 的标题，也可通过具名 slot （见下表）传入",
      "default": "—",
      "type": "string"
    },
    "width": {
      "value": "",
      "description": "Dialog 的宽度",
      "default": "—",
      "type": "string"
    },
    "fullscreen": {
      "value": "",
      "description": "是否为全屏 Dialog",
      "default": "false",
      "type": "boolean"
    },
    "top": {
      "value": "",
      "description": "Dialog CSS 中的 margin-top 值",
      "default": "—",
      "type": "string"
    },
    "modal": {
      "value": "",
      "description": "是否需要遮罩层",
      "default": "false",
      "type": "boolean"
    },
    "modal-append-to-body": {
      "value": "",
      "description": "遮罩层是否插入至 body 元素上，若为 false，则遮罩层会插入至 Dialog 的父元素上",
      "default": "false",
      "type": "boolean"
    },
    "append-to-body": {
      "value": "",
      "description": "Dialog 自身是否插入至 body 元素上。嵌套的 Dialog 必须指定该属性并赋值为 true",
      "default": "false",
      "type": "boolean"
    },
    "lock-scroll": {
      "value": "",
      "description": "是否在 Dialog 出现时将 body 滚动锁定",
      "default": "false",
      "type": "boolean"
    },
    "custom-class": {
      "value": "",
      "description": "Dialog 的自定义类名",
      "default": "—",
      "type": "string"
    },
    "close-on-click-modal": {
      "value": "",
      "description": "是否可以通过点击 modal 关闭 Dialog",
      "default": "false",
      "type": "boolean"
    },
    "close-on-press-escape": {
      "value": "",
      "description": "是否可以通过按下 ESC 关闭 Dialog",
      "default": "false",
      "type": "boolean"
    },
    "show-close": {
      "value": "",
      "description": "是否显示关闭按钮",
      "default": "false",
      "type": "boolean"
    },
    "before-close": {
      "value": "",
      "description": "关闭前的回调，会暂停 Dialog 的关闭",
      "default": "—",
      "type": "function(done)，done 用于关闭 Dialog"
    },
    "center": {
      "value": "",
      "description": "是否对头部和底部采用居中布局",
      "default": "false",
      "type": "boolean"
    },
    "destroy-on-close": {
      "value": "",
      "description": "关闭时销毁 Dialog 中的元素",
      "default": "false",
      "type": "boolean"
    },
    "事件名称": {
      "value": "",
      "description": "说明",
      "default": "",
      "type": "回调参数"
    },
    "open": {
      "value": "",
      "description": "Dialog 打开的回调",
      "default": "",
      "type": "—"
    },
    "opened": {
      "value": "",
      "description": "Dialog 打开动画结束时的回调",
      "default": "",
      "type": "—"
    },
    "close": {
      "value": "",
      "description": "Dialog 关闭的回调",
      "default": "",
      "type": "—"
    },
    "closed": {
      "value": "",
      "description": "Dialog 关闭动画结束时的回调",
      "default": "",
      "type": "—"
    }
  },
  "events": [],
  "link": "https://element.eleme.cn/#/zh-CN/component/dialog"
}
