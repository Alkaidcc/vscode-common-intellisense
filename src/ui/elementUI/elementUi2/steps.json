{
  "name": "ElSteps",
  "props": {
    "space": {
      "value": "",
      "description": "每个 step 的间距，不填写将自适应间距。支持百分比。",
      "default": "—",
      "type": "number | string"
    },
    "direction": {
      "value": [
        "vertical",
        "horizontal"
      ],
      "description": "显示方向",
      "default": "vertical | horizontal",
      "type": "string"
    },
    "active": {
      "value": "",
      "description": "设置当前激活步骤",
      "default": "—",
      "type": "number"
    },
    "process-status": {
      "value": [
        "wait",
        "process",
        "finish",
        "error",
        "success"
      ],
      "description": "设置当前步骤的状态",
      "default": "wait | process | finish | error | success",
      "type": "string"
    },
    "finish-status": {
      "value": [
        "wait",
        "process",
        "finish",
        "error",
        "success"
      ],
      "description": "设置结束步骤的状态",
      "default": "wait | process | finish | error | success",
      "type": "string"
    },
    "align-center": {
      "value": "",
      "description": "进行居中对齐",
      "default": "false",
      "type": "boolean"
    },
    "simple": {
      "value": "",
      "description": "是否应用简洁风格",
      "default": "false",
      "type": "boolean"
    }
  },
  "events": [],
  "link": "https://element.eleme.cn/#/zh-CN/component/steps"
}
