{
  "name": "ElRadioGroup",
  "props": {
    "value / v-model": {
      "value": "",
      "description": "绑定值",
      "default": "",
      "type": "string | number | boolean"
    },
    "size": {
      "value": [
        "medium",
        "small",
        "mini"
      ],
      "description": "单选框组尺寸，仅对按钮形式的 Radio 或带有边框的 Radio 有效",
      "default": "",
      "type": "string"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "text-color": {
      "value": "",
      "description": "按钮形式的 Radio 激活时的文本颜色",
      "default": "#ffffff",
      "type": "string"
    },
    "fill": {
      "value": "",
      "description": "按钮形式的 Radio 激活时的填充色和边框色",
      "default": "#409eff",
      "type": "string"
    }
  },
  "events": [
    {
      "name": "input",
      "description": "绑定值变化时触发的事件",
      "params": "选中的 Radio label 值"
    }
