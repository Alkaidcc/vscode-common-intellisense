{
  "name": "ElCarousel",
  "props": {
    "height": {
      "value": "",
      "description": "走马灯的高度",
      "default": "—",
      "type": "string"
    },
    "initial-index": {
      "value": "",
      "description": "初始状态激活的幻灯片的索引，从 0 开始",
      "default": "—",
      "type": "number"
    },
    "trigger": {
      "value": "",
      "description": "指示器的触发方式",
      "default": "click",
      "type": "string"
    },
    "autoplay": {
      "value": "",
      "description": "是否自动切换",
      "default": "false",
      "type": "boolean"
    },
    "interval": {
      "value": "",
      "description": "自动切换的时间间隔，单位为毫秒",
      "default": "—",
      "type": "number"
    },
    "indicator-position": {
      "value": [
        "outside",
        "none"
      ],
      "description": "指示器的位置",
      "default": "outside | none",
      "type": "string"
    },
    "arrow": {
      "value": [
        "always",
        "hover",
        "never"
      ],
      "description": "切换箭头的显示时机",
      "default": "always | hover | never",
      "type": "string"
    },
    "type": {
      "value": "",
      "description": "走马灯的类型",
      "default": "card",
      "type": "string"
    },
    "loop": {
      "value": "",
      "description": "是否循环显示",
      "default": "false",
      "type": "boolean"
    },
    "direction": {
      "value": [
        "horizontal",
        "vertical"
      ],
      "description": "走马灯展示的方向",
      "default": "horizontal | vertical",
      "type": "string"
    }
  },
  "events": [
    {
      "name": "change",
      "description": "幻灯片切换时触发",
      "callback": "目前激活的幻灯片的索引，原幻灯片的索引"
    }
  ],
  "link": "https://element.eleme.cn/#/zh-CN/component/carousel"
}
