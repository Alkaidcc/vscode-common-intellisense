{
  "name": "ElColorPicker",
  "props": {
    "value": {
      "value": "",
      "description": "绑定值",
      "default": "—",
      "type": "string"
    },
    "v-model": {
      "value": "",
      "description": "绑定值",
      "default": "—",
      "type": "string",
      "required": true
    },
    "disabled": {
      "value": "",
      "description": "是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "size": {
      "value": [
        "medium",
        "small",
        "mini"
      ],
      "description": "尺寸",
      "default": "medium | small | mini",
      "type": "string"
    },
    "show-alpha": {
      "value": "",
      "description": "是否支持透明度选择",
      "default": "false",
      "type": "boolean"
    },
    "color-format": {
      "value": [
        "hsl",
        "hsv",
        "hex",
        "rgb"
      ],
      "description": "写入 v-model 的颜色的格式",
      "default": "hsl | hsv | hex | rgb",
      "type": "string"
    },
    "popper-class": {
      "value": "",
      "description": "ColorPicker 下拉框的类名",
      "default": "—",
      "type": "string"
    },
    "predefine": {
      "value": "",
      "description": "预定义颜色",
      "default": "—",
      "type": "array"
    }
  },
  "events": [
    {
      "name": "change",
      "description": "当绑定值变化时触发",
      "params": "当前值"
    },
    {
      "name": "active-change",
      "description": "面板中当前显示的颜色发生改变时触发",
      "params": "当前显示的颜色值"
    }
  ],
  "link": "https://element.eleme.cn/#/zh-CN/component/color-picker"
