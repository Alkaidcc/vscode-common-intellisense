{
  "name": "Paragraph",
  "props": {
    "code": {
      "value": "",
      "description": "添加代码样式",
      "default": "false",
      "type": "boolean"
    },
    "copyable": {
      "value": "",
      "description": "是否可拷贝，为对象时可进行各种自定义",
      "default": "false",
      "type": "boolean | copyable"
    },
    "delete": {
      "value": "",
      "description": "添加删除线样式",
      "default": "false",
      "type": "boolean"
    },
    "disabled": {
      "value": "",
      "description": "禁用文本",
      "default": "false",
      "type": "boolean"
    },
    "editable": {
      "value": "",
      "description": "是否可编辑，为对象时可对编辑进行控制",
      "default": "false",
      "type": "boolean | editable"
    },
    "ellipsis": {
      "value": "",
      "description": "自动溢出省略，为对象时可设置省略行数、是否可展开、添加后缀等",
      "default": "false",
      "type": "boolean | Omit<ellipsis, 'expandable' | 'rows' | 'onExpand'>"
    },
    "mark": {
      "value": "",
      "description": "添加标记样式",
      "default": "false",
      "type": "boolean"
    },
    "italic": {
      "value": "",
      "description": "是否斜体",
      "default": "false",
      "type": "boolean"
    },
    "type": {
      "value": [
        "secondary",
        "success",
        "warning",
        "danger"
      ],
      "description": "文本类型",
      "default": "",
      "type": "secondary | success | warning | danger"
    },
    "underline": {
      "value": "",
      "description": "添加下划线样式",
      "default": "false",
      "type": "boolean"
    }
  },
  "events": [
    {
      "name": "onClick",
      "description": "点击 Paragraph 时的回调",
      "callback": "(event) => void"
    }
  ]
}
