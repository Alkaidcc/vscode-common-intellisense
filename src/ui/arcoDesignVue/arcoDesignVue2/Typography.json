  "typeDetail": {
    "EllipsisConfig": [
      {
        "name": "rows",
        "type": "number",
        "description": "显示省略的行数",
        "default": "1"
      },
      {
        "name": "expandable",
        "type": "boolean",
        "description": "是否支持展开/折叠",
        "default": "false"
      },
      {
        "name": "ellipsisStr",
        "type": "string",
        "description": "省略号",
        "default": "'...'"
      },
      {
        "name": "suffix",
        "type": "string",
        "description": "后缀",
        "default": "-"
      },
      {
        "name": "showTooltip",
        "type": "boolean | { type: 'tooltip' | 'popover'; props: Record<string, any> }",
