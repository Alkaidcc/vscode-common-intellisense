{
  "name": "VanSpace",
  "props": {
    "direction": {
      "value": ["vertical", "horizontal"],
      "description": "间距方向",
      "default": "horizontal",
      "type": "vertical | horizontal"
    },
    "size": {
      "value": "",
      "description": "间距大小，如 `20px` `2em`，默认单位为 `px`，支持数组形式来分别设置横向和纵向间距",
      "default": "8px",
      "type": "number | string | number[] | string[]"
    },
    "align": {
      "value": [
        "start",
        "end",
        "center",
        "baseline"
      ],
      "description": "设置子元素的对齐方式",
      "default": "-",
      "type": "start | end | center | baseline"
    },
    "wrap": {
      "value": "",
      "description": "是否自动换行，仅适用于水平方向排列",
      "default": "false",
      "type": "boolean"
    },
    "fill": {
      "value": "",
      "description": "是否让 Space 变为一个块级元素，填充整个父元素",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/space"
}
