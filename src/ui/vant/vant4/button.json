{
  "name": "VanButton",
  "props": {
    "type": {
      "value": ["primary", "success", "warning", "danger"],
      "description": "类型，可选值为 'primary' 'success' 'warning' 'danger'",
      "default": "default",
      "type": "string"
    },
    "size": {
      "value": ["large", "small", "mini"],
      "description": "尺寸，可选值为 `large` `small` `mini`",
      "default": "normal",
      "type": "string"
    },
    "text": {
      "value": "",
      "description": "按钮文字",
      "default": "-",
      "type": "string"
    },
    "color": {
      "value": "",
      "description": "按钮颜色，支持传入 `linear-gradient` 渐变色",
      "default": "-",
      "type": "string"
    },
    "icon": {
      "value": "",
      "description": "左侧图标名称或图片链接，等同于 Icon 组件的 [name 属性](#/zh-CN/icon#props)",
      "default": "-",
      "type": "string"
    },
    "icon-prefix": {
      "value": "",
      "description": "图标类名前缀，等同于 Icon 组件的 [class-prefix 属性](#/zh-CN/icon#props)",
      "default": "van-icon",
      "type": "string"
    },
    "icon-position": {
      "value": ["right", "left"],
      "description": "图标展示位置，可选值为 `right`",
      "default": "left",
      "type": "string"
    },
    "tag": {
      "value": "",
      "description": "按钮根节点的 HTML 标签",
      "default": "button",
      "type": "string"
    },
    "native-type": {
      "value": "",
      "description": "原生 button 标签的 type 属性",
      "default": "button",
      "type": "string"
    },
    "block": {
      "value": "",
      "description": "是否为块级元素",
      "default": "false",
      "type": "boolean"
    },
    "plain": {
      "value": "",
      "description": "是否为朴素按钮",
      "default": "false",
      "type": "boolean"
    },
    "square": {
      "value": "",
      "description": "是否为方形按钮",
      "default": "false",
      "type": "boolean"
    },
    "round": {
      "value": "",
      "description": "是否为圆形按钮",
      "default": "false",
      "type": "boolean"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用按钮",
      "default": "false",
      "type": "boolean"
    },
    "hairline": {
      "value": "",
      "description": "是否使用 0.5px 边框",
      "default": "false",
      "type": "boolean"
    },
    "loading": {
      "value": "",
      "description": "是否显示为加载状态",
      "default": "false",
      "type": "boolean"
    },
    "loading-text": {
      "value": "",
      "description": "加载状态提示文字",
      "default": "-",
      "type": "string"
    },
    "loading-type": {
      "value": ["circular", "spinner"],
      "description": "[加载图标类型](#/zh-CN/loading)，可选值为 `spinner`",
      "default": "circular",
      "type": "string"
    },
    "loading-size": {
      "value": "",
      "description": "加载图标大小，默认单位为 `px`",
      "default": "20px",
      "type": "number | string"
    },
    "url": {
      "value": "",
      "description": "点击后跳转的链接地址",
      "default": "-",
      "type": "string"
    },
    "to": {
      "value": "",
      "description": "点击后跳转的目标路由对象，等同于 Vue Router 的 [to 属性](https://router.vuejs.org/zh/api/interfaces/RouterLinkProps.html#Properties-to)",
      "default": "-",
      "type": "string | object"
    },
    "replace": {
      "value": "",
      "description": "是否在跳转时替换当前页面历史",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "click",
      "description": "点击按钮，且按钮状态不为加载或禁用时触发",
      "params": "event: MouseEvent"
    },
    {
      "name": "touchstart",
      "description": "开始触摸按钮时触发",
      "params": "event: TouchEvent"
    }
  ],
  "link": "https://vant-ui.github.io/vant/#/zh-CN/button"
}
