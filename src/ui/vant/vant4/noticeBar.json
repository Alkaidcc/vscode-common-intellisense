{
  "name": "VanNoticeBar",
  "props": {
    "mode": {
      "value": ["closeable", "link"],
      "description": "通知栏模式，可选值为 `closeable` link",
      "default": "''",
      "type": "string"
    },
    "text": {
      "value": "",
      "description": "通知文本内容",
      "default": "''",
      "type": "string"
    },
    "color": {
      "value": "",
      "description": "通知文本颜色",
      "default": "#ed6a0c",
      "type": "string"
    },
    "background": {
      "value": "",
      "description": "滚动条背景",
      "default": "#fffbe8",
      "type": "string"
    },
    "left-icon": {
      "value": "",
      "description": "左侧图标名称或图片链接，等同于 Icon 组件的 [name 属性](#/zh-CN/icon#props)",
      "default": "-",
      "type": "string"
    },
    "delay": {
      "value": "",
      "description": "动画延迟时间 (s)",
      "default": "1",
      "type": "number | string"
    },
    "speed": {
      "value": "",
      "description": "滚动速率 (px/s)",
      "default": "60",
      "type": "number | string"
    },
    "scrollable": {
      "value": "",
      "description": "是否开启滚动播放，内容长度溢出时默认开启",
      "default": "false",
      "type": "boolean"
    },
    "wrapable": {
      "value": "",
      "description": "是否开启文本换行，只在禁用滚动时生效",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [{ "name": "reset", "description": "重置通知栏到初始状态", "params": "-" }],
  "typeDetail": {},
  "events": [
    {
      "name": "click",
      "description": "点击通知栏时触发",
      "params": "点击通知栏时触发"
    },
    {
      "name": "close",
      "description": "关闭通知栏时触发",
      "params": "关闭通知栏时触发"
    },
    {
      "name": "repla",
      "description": "",
      "params": ""
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/notice-bar"
