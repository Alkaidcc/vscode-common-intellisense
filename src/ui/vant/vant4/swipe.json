{
  "name": "VanSwipe",
  "props": {
    "autoplay": {
      "value": "",
      "description": "自动轮播间隔，单位为 ms",
      "default": "-",
      "type": "number | string"
    },
    "duration": {
      "value": "",
      "description": "动画时长，单位为 ms",
      "default": "500",
      "type": "number | string"
    },
    "initial-swipe": {
      "value": "",
      "description": "初始位置索引值",
      "default": "0",
      "type": "number | string"
    },
    "width": {
      "value": "",
      "description": "滑块宽度，单位为 `px`",
      "default": "auto",
      "type": "number | string"
    },
    "height": {
      "value": "",
      "description": "滑块高度，单位为 `px`",
      "default": "auto",
      "type": "number | string"
    },
    "loop": {
      "value": "",
      "description": "是否开启循环播放",
      "default": "true",
      "type": "boolean"
    },
    "show-indicators": {
      "value": "",
      "description": "是否显示指示器",
      "default": "true",
      "type": "boolean"
    },
    "vertical": {
      "value": "",
      "description": "是否为纵向滚动",
      "default": "false",
      "type": "boolean"
    },
    "touchable": {
      "value": "",
      "description": "是否可以通过手势滑动",
      "default": "true",
      "type": "boolean"
    },
    "stop-propagation": {
      "value": "",
      "description": "是否阻止滑动事件冒泡",
      "default": "true",
      "type": "boolean"
    },
    "lazy-render": {
      "value": "",
      "description": "是否延迟渲染未展示的轮播",
      "default": "false",
      "type": "boolean"
    },
    "indicator-color": {
      "value": "",
      "description": "指示器颜色",
      "default": "#1989fa",
      "type": "string"
    }
  },
  "methods": [{ "name": "prev", "description": "切换到上一轮播", "params": "-" }, { "name": "next", "description": "切换到下一轮播", "params": "-" }, { "name": "swipeTo", "description": "切换到指定位置", "params": "index: number, options: SwipeToOptions" }, { "name": "resize", "description": "外层元素大小或组件显示状态变化时，可以调用此方法来触发重绘", "params": "-" }],
  "typeDetail": { "SwipeToOptions": [{ "name": "immediate", "description": "是否跳过动画", "type": "boolean" }] },
  "events": [
    {
      "name": "change",
      "description": "每一页轮播结束后触发",
      "params": "每一页轮播结束后触发"
    },
    {
      "name": "drag-start",
      "description": "当用户开始拖动轮播组件时触发",
      "params": "当用户开始拖动轮播组件时触发"
    },
    {
      "name": "drag-end",
      "description": "当用户结束拖动轮播组件时触发",
      "params": "当用户结束拖动轮播组件时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/swipe"
}
