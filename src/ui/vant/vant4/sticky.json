{
  "name": "VanSticky",
  "props": {
    "position": {
      "value": ["top", "bottom"],
      "description": "吸附位置，可选值为 bottom",
      "default": "top",
      "type": "string"
    },
    "offset-top": {
      "value": "",
      "description": "吸顶时与顶部的距离，支持 `px` `vw` `vh` `rem` 单位，默认 px",
      "default": "0",
      "type": "number | string"
    },
    "offset-bottom": {
      "value": "",
      "description": "吸底时与底部的距离，支持 `px` `vw` `vh` `rem` 单位，默认 px",
      "default": "0",
      "type": "number | string"
    },
    "z-index": {
      "value": "",
      "description": "吸顶时的 z-index",
      "default": "99",
      "type": "number | string"
    },
    "container": {
      "value": "",
      "description": "容器对应的 HTML 节点",
      "default": "-",
      "type": "Element"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "当吸顶状态改变时触发",
      "params": "当吸顶状态改变时触发"
    },
    {
      "name": "scroll",
      "description": "滚动时触发",
      "params": "滚动时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/sticky"
}
