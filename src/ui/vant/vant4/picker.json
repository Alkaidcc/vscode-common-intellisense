{
  "name": "VanPicker",
  "props": {
    ":columns": {
      "value": "",
      "description": "对象数组，配置每一列显示的数据",
      "default": "[]",
      "type": "PickerOption[] | PickerOption[][]"
    },
    "columns-field-names": {
      "value": "",
      "description": "自定义 `columns` 结构中的字段",
      "default": "{ text: 'text', value: 'value', children: 'children' }",
      "type": "object"
    },
    "title": {
      "value": "",
      "description": "顶部栏标题",
      "default": "-",
      "type": "string"
    },
    "confirm-button-text": {
      "value": "",
      "description": "确认按钮文字",
      "default": "确认",
      "type": "string"
    },
    "cancel-button-text": {
      "value": "",
      "description": "取消按钮文字",
      "default": "取消",
      "type": "string"
    },
    "toolbar-position": {
      "value": ["top", "bottom"],
      "description": "顶部栏位置，可选值为 `bottom`",
      "default": "top",
      "type": "string"
    },
    "loading": {
      "value": "",
      "description": "是否显示加载状态",
      "default": "false",
      "type": "boolean"
    },
    "show-toolbar": {
      "value": "",
      "description": "是否显示顶部栏",
      "default": "true",
      "type": "boolean"
    },
    "allow-html": {
      "value": "",
      "description": "是否允许选项内容中渲染 HTML",
      "default": "false",
      "type": "boolean"
    },
    "option-height": {
      "value": "",
      "description": "选项高度，支持 `px` `vw` `vh` `rem` 单位，默认 `px`",
      "default": "44",
      "type": "number | string"
    },
    "visible-option-num": {
      "value": "",
      "description": "可见的选项个数",
      "default": "6",
      "type": "number | string"
    },
    "swipe-duration": {
      "value": "",
      "description": "快速滑动时惯性滚动的时长，单位 `ms`",
      "default": "1000",
      "type": "number | string"
    }
  },
  "methods": [{ "name": "confirm", "description": "停止惯性滚动并触发 `confirm` 事件", "params": "-" }, { "name": "getSelectedOptions", "description": "获取当前选中的选项", "params": "-" }],
  "typeDetail": {
    "PickerOption": [
      {
        "name": "text",
        "description": "选项文字内容",
        "type": "string | number"
      },
      {
        "name": "value",
        "description": "选项对应的值",
        "type": "string | number"
      },
      {
        "name": "disabled",
        "description": "是否禁用选项",
        "type": "boolean"
      },
      {
        "name": "children",
        "description": "级联选项",
        "type": "PickerOption[]"
      },
      {
        "name": "className",
        "description": "选项额外类名",
        "type": "string | Array | object"
      }
    ]
  },
  "events": [
    {
      "name": "confirm",
      "description": "点击完成按钮时触发",
      "params": "点击完成按钮时触发"
    },
    {
      "name": "cancel",
      "description": "点击取消按钮时触发",
      "params": "点击取消按钮时触发"
    },
    {
      "name": "change",
      "description": "选中的选项改变时触发",
      "params": "选中的选项改变时触发"
    },
    {
      "name": "click-option",
      "description": "点击选项时触发",
      "params": "点击选项时触发"
    },
    {
      "name": "scroll-into ",
      "description": "当用户通过点击或拖拽让一个选项滚动到中间的选择区域时触发",
      "params": "当用户通过点击或拖拽让一个选项滚动到中间的选择区域时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/picker"
}
