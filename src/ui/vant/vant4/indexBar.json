{
  "name": "VanIndexBar",
  "props": {
    "index-list": {
      "value": "",
      "description": "索引字符列表",
      "default": "A-Z",
      "type": "(string | number)[]"
    },
    "z-index": {
      "value": "",
      "description": "z-index 层级",
      "default": "1",
      "type": "number | string"
    },
    "sticky": {
      "value": "",
      "description": "是否开启锚点自动吸顶",
      "default": "true",
      "type": "boolean"
    },
    "sticky-offset-top": {
      "value": "",
      "description": "锚点自动吸顶时与顶部的距离",
      "default": "0",
      "type": "number"
    },
    "highlight-color": {
      "value": "",
      "description": "索引字符高亮颜色",
      "default": "#1989fa",
      "type": "string"
    },
    "teleport": {
      "value": "",
      "description": "指定索引栏挂载的节点",
      "default": "-",
      "type": "string | Element"
    }
  },
  "methods": [{ "name": "scrollTo", "description": "滚动到指定锚点", "params": "index: number | string" }],
  "typeDetail": {},
  "events": [
    {
      "name": "select",
      "description": "点击索引栏的字符时触发",
      "params": "点击索引栏的字符时触发"
    },
    {
      "name": "change",
      "description": "当前高亮的索引字符变化时触发",
      "params": "当前高亮的索引字符变化时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/index-bar"
}
