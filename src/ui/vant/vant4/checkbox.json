{
  "name": "VanCheckbox",
  "props": {
    "v-model": {
      "value": "",
      "description": "是否为选中状态",
      "default": "false",
      "type": "boolean"
    },
    "name": {
      "value": "",
      "description": "标识符，通常为一个唯一的字符串或数字",
      "default": "-",
      "type": "any"
    },
    "shape": {
      "value": ["round", "square"],
      "description": "形状，可选值为 `square`",
      "default": "round",
      "type": "string"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用复选框",
      "default": "false",
      "type": "boolean"
    },
    "label-disabled": {
      "value": "",
      "description": "是否禁用复选框文本点击",
      "default": "false",
      "type": "boolean"
    },
    "label-position": {
      "value": ["right", "left"],
      "description": "文本位置，可选值为 `left`",
      "default": "right",
      "type": "string"
    },
    "icon-size": {
      "value": "",
      "description": "图标大小，默认单位为 `px`",
      "default": "20px",
      "type": "number | string"
    },
    "checked-color": {
      "value": "",
      "description": "选中状态颜色",
      "default": "#1989fa",
      "type": "string"
    },
    "bind-group": {
      "value": "",
      "description": "是否与复选框组绑定",
      "default": "true",
      "type": "boolean"
    }
  },
  "methods": [{ "name": "toggle", "description": "切换选中状态，传 `true` 为选中，`false` 为取消选中，不传参为取反", "params": "checked?: boolean" }],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "当绑定值变化时触发的事件",
      "params": "当绑定值变化时触发的事件"
    },
    {
      "name": "click",
      "description": "点击复选框时触发",
      "params": "点击复选框时触发"
    }
  ],
  "link": "https://vant-ui.github.io/vant/#/zh-CN/checkbox"
}
