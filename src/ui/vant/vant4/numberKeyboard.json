    },
    {
      "name": "show",
      "description": "键盘完全弹出时触发",
      "params": "键盘完全弹出时触发"
    },
    {
      "name": "hide",
      "description": "键盘完全收起时触发",
      "params": "键盘完全收起时触发"
    }
