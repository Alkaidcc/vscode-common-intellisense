{
  "name": "VanNumberKeyboard",
  "props": {
    "v-model": {
      "value": "",
      "description": "当前输入值",
      "default": "-",
      "type": "string",
      "required":true
    },
    ":show": {
      "value": "",
      "description": "是否显示键盘",
      "default": "false",
      "type": "boolean"
    },
    "title": {
      "value": "",
      "description": "键盘标题",
      "default": "-",
      "type": "string"
    },
    "theme": {
      "value": ["default", "custom"],
      "description": "样式风格，可选值为 custom",
      "default": "default",
      "type": "string"
    },
    "maxlength": {
      "value": "",
      "description": "输入值最大长度",
      "default": "Infinity",
      "type": "number | string"
    },
    "transition": {
      "value": "",
      "description": "是否开启过场动画",
      "default": "true",
      "type": "boolean"
    },
    "z-index": {
      "value": "",
      "description": "键盘 z-index 层级",
      "default": "100",
      "type": "number | string"
    },
    "extra-key": {
      "value": "",
      "description": "底部额外按键的内容",
      "default": "''",
      "type": "string | string[]"
    },
    "close-button-text": {
      "value": "",
      "description": "关闭按钮文字，空则不展示",
      "default": "-",
      "type": "string"
    },
    "delete-button-text": {
      "value": "",
      "description": "删除按钮文字，空则展示删除图标",
      "default": "-",
      "type": "string"
    },
    "close-button-loading": {
      "value": "",
      "description": "是否将关闭按钮设置为加载中状态，仅在 `theme=\"custom\"` 时有效",
      "default": "false",
      "type": "boolean"
    },
    "show-delete-key": {
      "value": "",
      "description": "是否展示删除图标",
      "default": "true",
      "type": "boolean"
    },
    "blur-on-close": {
      "value": "",
      "description": "是否在点击关闭按钮时触发 blur 事件",
      "default": "true",
      "type": "boolean"
    },
    "hide-on-click-outside": {
      "value": "",
      "description": "是否在点击外部时收起键盘",
      "default": "true",
      "type": "boolean"
    },
    "teleport": {
      "value": "",
      "description": "指定挂载的节点，等同于 Teleport 组件的 [to 属性](https://cn.vuejs.org/api/built-in-components.html#teleport)",
      "default": "-",
      "type": "string | Element"
    },
    "safe-area-inset-bottom": {
      "value": "",
      "description": "是否开启[底部安全区适配](#/zh-CN/advanced-usage#di-bu-an-quan-qu-gua-pei)",
      "default": "true",
      "type": "boolean"
    },
    "random-key-order": {
      "value": "",
      "description": "是否将通过随机顺序展示按键",
      "default": "false",
      "type": "boolean"
    }
