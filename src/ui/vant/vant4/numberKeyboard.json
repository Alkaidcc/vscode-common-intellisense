  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "input",
      "description": "点击按键时触发",
      "params": "点击按键时触发"
    },
    {
      "name": "delete",
      "description": "点击删除键时触发",
      "params": "点击删除键时触发"
    },
    {
      "name": "close",
      "description": "点击关闭按钮时触发",
      "params": "点击关闭按钮时触发"
    },
    {
      "name": "blur",
      "description": "点击关闭按钮或非键盘区域时触发",
