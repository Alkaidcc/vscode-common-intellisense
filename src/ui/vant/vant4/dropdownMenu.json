{
  "name": "VanDropdownMenu",
  "props": {
    "active-color": {
      "value": "",
      "description": "菜单标题和选项的选中态颜色",
      "default": "#1989fa",
      "type": "string"
    },
    "direction": {
      "value": ["up", "down"],
      "description": "菜单展开方向，可选值为`up`",
      "default": "down",
      "type": "string"
    },
    "z-index": {
      "value": "",
      "description": "菜单栏 z-index 层级",
      "default": "10",
      "type": "number | string"
    },
    "duration": {
      "value": "",
      "description": "动画时长，单位秒，设置为 0 可以禁用动画",
      "default": "0.2",
      "type": "number | string"
    },
    "overlay": {
      "value": "",
      "description": "是否显示遮罩层",
