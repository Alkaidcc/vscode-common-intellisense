      "type": "boolean"
    },
    "close-on-click-overlay": {
      "value": "",
      "description": "是否在点击遮罩层后关闭菜单",
      "default": "true",
      "type": "boolean"
    },
    "close-on-click-outside": {
      "value": "",
      "description": "是否在点击外部元素后关闭菜单",
      "default": "true",
      "type": "boolean"
