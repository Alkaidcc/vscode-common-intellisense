{
  "name": "VanActionSheet",
  "props": {
    "v-model:show": {
      "value": "",
      "description": "是否显示动作面板",
      "default": "false",
      "type": "boolean"
    },
    "actions": {
      "value": "",
      "description": "面板选项列表",
      "default": "[]",
      "type": "ActionSheetAction[]"
    },
    "title": {
      "value": "",
      "description": "顶部标题",
      "default": "-",
      "type": "string"
    },
    "cancel-text": {
      "value": "",
      "description": "取消按钮文字",
      "default": "-",
      "type": "string"
    },
    "description": {
      "value": "",
      "description": "选项上方的描述信息",
      "default": "-",
      "type": "string"
    },
    "closeable": {
      "value": "",
      "description": "是否显示关闭图标",
      "default": "true",
      "type": "boolean"
    },
    "close-icon": {
      "value": "",
      "description": "关闭图标名称或图片链接，等同于 Icon 组件的 [name 属性](#/zh-CN/icon#props)",
      "default": "cross",
      "type": "string"
    },
    "duration": {
      "value": "",
      "description": "动画时长，单位秒，设置为 0 可以禁用动画",
      "default": "0.3",
      "type": "number | string"
    },
    "z-index": {
      "value": "",
      "description": "将面板的 z-index 层级设置为一个固定值",
      "default": "2000+",
      "type": "number | string"
    },
    "round": {
      "value": "",
      "description": "是否显示圆角",
      "default": "true",
      "type": "boolean"
    },
    "overlay": {
      "value": "",
      "description": "是否显示遮罩层",
      "default": "true",
      "type": "boolean"
    },
    "overlay-class": {
      "value": "",
      "description": "自定义遮罩层类名",
      "default": "-",
      "type": "string | Array | object"
    },
    "overlay-style": {
      "value": "",
      "description": "自定义遮罩层样式",
      "default": "-",
      "type": "object"
    },
    "lock-scroll": {
      "value": "",
      "description": "是否锁定背景滚动",
      "default": "true",
      "type": "boolean"
    },
    "lazy-render": {
      "value": "",
      "description": "是否在显示弹层时才渲染节点",
      "default": "true",
      "type": "boolean"
    },
    "close-on-popstate": {
      "value": "",
      "description": "是否在页面回退时自动关闭",
      "default": "true",
      "type": "boolean"
    },
    "close-on-click-action": {
      "value": "",
      "description": "是否在点击选项后关闭",
      "default": "false",
      "type": "boolean"
    },
    "close-on-click-overlay": {
      "value": "",
      "description": "是否在点击遮罩层后关闭",
      "default": "true",
      "type": "boolean"
    },
    "safe-area-inset-bottom": {
      "value": "",
      "description": "是否开启[底部安全区适配](#/zh-CN/advanced-usage#di-bu-an-quan-qu-gua-pei)",
      "default": "true",
      "type": "boolean"
    },
    "teleport": {
      "value": "",
      "description": "指定挂载的节点，等同于 Teleport 组件的 [to 属性](https://cn.vuejs.org/api/built-in-components.html#teleport)",
      "default": "-",
      "type": "string | Element"
    },
    "before-close": {
      "value": "",
      "description": "关闭前的回调函数，返回 `false` 可阻止关闭，支持返回 Promise",
      "default": "-",
      "type": "(action: string) => boolean | Promise\\<boolean\\>"
    }
  },
  "methods": [],
  "typeDetail": {
    "Action": [
      {
        "name": "name",
        "description": "标题",
        "type": "string"
      },
      {
        "name": "subname",
        "description": "二级标题",
        "type": "string"
      },
      {
        "name": "color",
        "description": "选项文字颜色",
        "type": "string"
      },
      {
        "name": "className",
        "description": "为对应列添加额外的 class",
        "type": "string | Array | object"
      },
      {
        "name": "loading",
        "description": "是否为加载状态",
        "type": "boolean"
      },
      {
        "name": "disabled",
        "description": "是否为禁用状态",
        "type": "boolean"
      },
      {
        "name": "callback",
        "description": "点击时触发的回调函数",
        "type": "action: ActionSheetAction"
      }
    ]
  },
  "events": [
    {
      "name": "select",
      "description": "点击选项时触发，禁用或加载状态下不会触发",
      "params": "action: ActionSheetAction, index: number"
    },
    {
      "name": "cancel",
      "description": "点击取消按钮时触发",
      "params": "-"
    },
    {
      "name": "open",
      "description": "打开面板时触发",
      "params": "-"
    },
    {
      "name": "close",
      "description": "关闭面板时触发",
      "params": "-"
    },
    {
      "name": "opened",
      "description": "打开面板且动画结束后触发",
      "params": "-"
    },
    {
      "name": "closed",
      "description": "关闭面板且动画结束后触发",
      "params": "-"
    },
    {
      "name": "click-overlay",
      "description": "点击遮罩层时触发",
      "params": "event: MouseEvent"
    }
  ],
  "link": "https://vant-docs.netlify.app/#/zh-CN/action-sheet"
}
