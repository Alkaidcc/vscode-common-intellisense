{
  "name": "VanPopup",
  "props": {
    "v-model:show": {
      "value": "",
      "description": "是否显示弹出层",
      "default": "false",
      "type": "boolean"
    },
    "overlay": {
      "value": "",
      "description": "是否显示遮罩层",
      "default": "true",
      "type": "boolean"
    },
    "position": {
      "value": ["top", "bottom", "right", "left"],
      "description": "弹出位置，可选值为 `top` `bottom` `right` left",
      "default": "center",
      "type": "string"
    },
    "overlay-class": {
      "value": "",
      "description": "自定义遮罩层类名",
      "default": "-",
      "type": "string | Array | object"
    },
    "overlay-style": {
      "value": "",
      "description": "自定义遮罩层样式",
      "default": "-",
      "type": "object"
    },
    "duration": {
      "value": "",
      "description": "动画时长，单位秒，设置为 0 可以禁用动画",
      "default": "0.3",
      "type": "number | string"
    },
    "z-index": {
      "value": "",
      "description": "将弹窗的 z-index 层级设置为一个固定值",
      "default": "2000+",
      "type": "number | string"
    },
    "round": {
      "value": "",
      "description": "是否显示圆角",
      "default": "false",
      "type": "boolean"
    },
    "lock-scroll": {
      "value": "",
      "description": "是否锁定背景滚动",
      "default": "true",
      "type": "boolean"
    },
    "lazy-render": {
      "value": "",
      "description": "是否在显示弹层时才渲染节点",
      "default": "true",
      "type": "boolean"
    },
    "close-on-popstate": {
      "value": "",
      "description": "是否在页面回退时自动关闭",
      "default": "false",
      "type": "boolean"
    },
    "close-on-click-overlay": {
      "value": "",
      "description": "是否在点击遮罩层后关闭",
      "default": "true",
      "type": "boolean"
    },
    "closeable": {
      "value": "",
      "description": "是否显示关闭图标",
      "default": "false",
      "type": "boolean"
    },
    "close-icon": {
      "value": "",
      "description": "关闭图标名称或图片链接，等同于 Icon 组件的 [name 属性](#/zh-CN/icon#props)",
      "default": "cross",
      "type": "string"
    },
    "close-icon-position": {
      "value": ["top-left", "top-right", "bottom-left", "bottom-right"],
      "description": "关闭图标位置，可选值为 `top-left` `bottom-left` `bottom-right`",
      "default": "top-right",
      "type": "string"
    },
    "before-close": {
      "value": "",
      "description": "关闭前的回调函数，返回 `false` 可阻止关闭，支持返回 Promise",
      "default": "-",
      "type": "(action: string) => boolean | Promise\\<boolean\\>"
    },
    "icon-prefix": {
      "value": "",
      "description": "图标类名前缀，等同于 Icon 组件的 [class-prefix 属性](#/zh-CN/icon#props)",
      "default": "van-icon",
      "type": "string"
    },
    "transition": {
      "value": "",
      "description": "动画类名，等价于 [transition](https://cn.vuejs.org/api/built-in-components.html#transition) 的 `name` 属性",
      "default": "-",
      "type": "string"
    },
    "transition-appear": {
      "value": "",
      "description": "是否在初始渲染时启用过渡动画",
      "default": "false",
      "type": "boolean"
    },
    "teleport": {
      "value": "",
      "description": "指定挂载的节点，等同于 Teleport 组件的 [to 属性](https://cn.vuejs.org/api/built-in-components.html#teleport)",
      "default": "-",
      "type": "string | Element"
    },
    "safe-area-inset-top": {
      "value": "",
      "description": "是否开启[顶部安全区适配](#/zh-CN/advanced-usage#di-bu-an-quan-qu-gua-pei)",
      "default": "false",
      "type": "boolean"
    },
    "safe-area-inset-bottom": {
      "value": "",
      "description": "是否开启[底部安全区适配](#/zh-CN/advanced-usage#di-bu-an-quan-qu-gua-pei)",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "click",
      "description": "点击弹出层时触发",
      "params": "点击弹出层时触发"
    },
    {
      "name": "click-overlay",
      "description": "点击遮罩层时触发",
      "params": "点击遮罩层时触发"
    },
    {
      "name": "click-close-icon",
      "description": "点击关闭图标时触发",
      "params": "点击关闭图标时触发"
    },
    {
      "name": "open",
      "description": "打开弹出层时立即触发",
      "params": "打开弹出层时立即触发"
    },
    {
      "name": "close",
      "description": "关闭弹出层时立即触发",
      "params": "关闭弹出层时立即触发"
    },
    {
      "name": "opened",
      "description": "打开弹出层且动画结束后触发",
      "params": "打开弹出层且动画结束后触发"
    },
    {
      "name": "closed",
      "description": "关闭弹出层且动画结束后触发",
      "params": "关闭弹出层且动画结束后触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/popup"
}
