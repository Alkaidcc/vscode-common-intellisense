{
  "name": "VanPagination",
  "props": {
    "v-model": {
      "value": "",
      "description": "当前页码",
      "default": "-",
      "type": "number"
    },
    "mode": {
      "value": "",
      "description": "显示模式，可选值为 `simple`",
      "default": "multi",
      "type": "string"
    },
    "prev-text": {
      "value": "",
      "description": "上一页按钮文字",
      "default": "上一页",
      "type": "string"
    },
    "next-text": {
      "value": "",
      "description": "下一页按钮文字",
      "default": "下一页",
      "type": "string"
    },
    "page-count": {
      "value": "",
      "description": "总页数",
      "default": "根据页数计算",
      "type": "number | string"
    },
    "total-items": {
      "value": "",
      "description": "总记录数",
      "default": "0",
      "type": "number | string"
    },
    "items-per-page": {
      "value": "",
      "description": "每页记录数",
      "default": "10",
      "type": "number | string"
    },
    "show-page-size": {
      "value": "",
      "description": "显示的页码个数",
      "default": "5",
      "type": "number | string"
    },
    "force-ellipses": {
      "value": "",
      "description": "是否显示省略号",
      "default": "false",
      "type": "boolean"
    },
    "show-prev-button": {
      "value": "",
      "description": "是否展示上一页按钮",
      "default": "true",
      "type": "boolean"
    },
    "show-next-button": {
      "value": "",
      "description": "是否展示下一页按钮",
      "default": "true",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "页码改变时触发",
      "params": "页码改变时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/pagination"
}
