{
  "name": "VanDropdownItem",
  "props": {
    "v-model": {
      "value": "",
      "description": "当前选中项对应的 value",
      "default": "-",
      "type": "number | string",
      "required": true
    },
    "title": {
      "value": "",
      "description": "菜单项标题",
      "default": "当前选中项文字",
      "type": "string"
    },
    ":options": {
      "value": "",
      "description": "选项数组",
      "default": "[]",
      "type": "Option[]"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用菜单",
      "default": "false",
      "type": "boolean"
    },
    "lazy-render": {
      "value": "",
      "description": "是否在首次展开时才渲染菜单内容",
      "default": "true",
      "type": "boolean"
    },
    "title-class": {
      "value": "",
      "description": "标题额外类名",
      "default": "-",
      "type": "string | Array | object"
    },
    "teleport": {
      "value": "",
      "description": "指定挂载的节点，等同于 Teleport 组件的 [to 属性](https://cn.vuejs.org/api/built-in-components.html#teleport)",
      "default": "-",
      "type": "string | Element"
    }
  },
  "methods": [{ "name": "toggle", "description": "切换菜单展示状态，传 `true` 为显示，`false` 为隐藏，不传参为取反", "params": "show?: boolean" }],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "点击选项导致 value 变化时触发",
      "params": "点击选项导致 value 变化时触发"
    },
    {
      "name": "open",
      "description": "打开菜单栏时触发",
      "params": "打开菜单栏时触发"
    },
    {
      "name": "close",
      "description": "关闭菜单栏时触发",
      "params": "关闭菜单栏时触发"
    },
    {
      "name": "opened",
      "description": "打开菜单栏且动画结束后触发",
      "params": "打开菜单栏且动画结束后触发"
    },
    {
      "name": "closed",
      "description": "关闭菜单栏且动画结束后触发",
      "params": "关闭菜单栏且动画结束后触发"
    }
  ],
  "link": "https://vant-docs.netlify.app/#/zh-CN/dropdown-menu"
}
