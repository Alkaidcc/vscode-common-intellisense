{
  "name": "VanImage",
  "props": {
    "src": {
      "value": "",
      "description": "图片链接",
      "default": "-",
      "type": "string"
    },
    "fit": {
      "value": "",
      "description": "图片填充模式，等同于原生的 [object-fit](https://developer.mozilla.org/zh-CN/docs/Web/CSS/object-fit) 属性",
      "default": "fill",
      "type": "string"
    },
    "position": {
      "value": ["center", "top", "right", "bottom", "left"],
      "description": "图片位置，等同于原生的 [object-position](https://developer.mozilla.org/zh-CN/docs/Web/CSS/object-position) 属性，可选值为 `top` `right` `bottom` `left` 或 `string`",
      "default": "center",
      "type": "string"
    },
    "alt": {
      "value": "",
      "description": "替代文本",
      "default": "-",
      "type": "string"
    },
    "width": {
      "value": "",
      "description": "宽度，默认单位为 `px`",
      "default": "-",
      "type": "number | string"
    },
    "height": {
      "value": "",
      "description": "高度，默认单位为 `px`",
      "default": "-",
      "type": "number | string"
    },
    "radius": {
      "value": "",
      "description": "圆角大小，默认单位为 `px`",
      "default": "0",
      "type": "number | string"
    },
    "round": {
      "value": "",
      "description": "是否显示为圆形",
      "default": "false",
      "type": "boolean"
    },
    "block 3.6.3": {
      "value": "",
      "description": "是否将根节点设置为块级元素，默认情况下为 `inline-block` 元素",
      "default": "false",
      "type": "boolean"
    },
    "lazy-load": {
      "value": "",
      "description": "是否开启图片懒加载，须配合 [Lazyload](#/zh-CN/lazyload) 组件使用",
      "default": "false",
      "type": "boolean"
    },
    "show-error": {
      "value": "",
      "description": "是否展示图片加载失败提示",
      "default": "true",
      "type": "boolean"
    },
    "show-loading": {
      "value": "",
      "description": "是否展示图片加载中提示",
      "default": "true",
      "type": "boolean"
    },
    "error-icon": {
      "value": "",
      "description": "失败时提示的图标名称或图片链接，等同于 Icon 组件的 [name 属性](#/zh-CN/icon#props)",
      "default": "photo-fail",
      "type": "string"
    },
    "loading-icon": {
      "value": "",
      "description": "加载时提示的图标名称或图片链接，等同于 Icon 组件的 [name 属性](#/zh-CN/icon#props)",
      "default": "photo",
      "type": "string"
    },
    "icon-size": {
      "value": "",
      "description": "加载图标和失败图标的大小",
      "default": "32px",
      "type": "number | string"
    },
    "icon-prefix": {
      "value": "",
      "description": "图标类名前缀，等同于 Icon 组件的 [class-prefix 属性](#/zh-CN/icon#props)",
      "default": "van-icon",
      "type": "string"
    }
  },
  "methods": [],
  "typeDetail": {
    "Fit": [
      {
        "name": "contain",
        "description": "保持宽高缩放图片，使图片的长边能完全显示出来",
        "type": ""
      },
      {
        "name": "cover",
        "description": "保持宽高缩放图片，使图片的短边能完全显示出来，裁剪长边",
        "type": ""
      },
      {
        "name": "fill",
        "description": "拉伸图片，使图片填满元素",
        "type": ""
      },
      {
        "name": "none",
        "description": "保持图片原有尺寸",
        "type": ""
      },
      {
        "name": "scale-down",
        "description": "取 `none` 或 `contain` 中较小的一个",
        "type": ""
      }
    ]
  },
  "events": [
    {
      "name": "click",
      "description": "点击图片时触发",
      "params": "点击图片时触发"
    },
    {
      "name": "load",
      "description": "图片加载完毕时触发",
      "params": "图片加载完毕时触发"
    },
    {
      "name": "error",
      "description": "图片加载失败时触发",
      "params": "图片加载失败时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/image"
}
