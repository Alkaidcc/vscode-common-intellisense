{
  "name": "VanBackTop",
  "props": {
    "target": {
      "value": "",
      "description": "触发滚动的目标对象，支持传入选择器或 DOM 元素，默认最近的父级滚动容器",
      "default": "-",
      "type": "string | HTMLElement"
    },
    "right": {
      "value": "",
      "description": "距离页面右侧的距离，默认单位为 px",
      "default": "30",
      "type": "number | string"
    },
    "bottom": {
      "value": "",
      "description": "距离页面底部的距离，默认单位为 px",
      "default": "40",
      "type": "number | string"
    },
    "offset": {
      "value": "",
      "description": "滚动高度达到此参数值时才显示组件",
      "default": "200",
      "type": "number"
    },
    "teleport": {
      "value": "",
      "description": "指定挂载的节点，等同于 Teleport 组件的 [to 属性](https://cn.vuejs.org/api/built-in-components.html#teleport)",
      "default": "body",
      "type": "string | Element"
    },
    "immediate": {
      "value": "",
      "description": "是否瞬间滚动到顶部",
      "default": "false",
      "type": "boolean"
    },
    "z-index": {
      "value": "",
      "description": "设置组件的 z-index 层级",
      "default": "100",
      "type": "number | string"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "click",
      "description": "点击组件时触发",
      "params": "event: MouseEvent"
    }
  ],
  "link": "https://vant-docs.netlify.app/#/zh-CN/back-top"
}
