{
  "name": "VanRollText",
  "props": {
    "start-num": {
      "value": "",
      "description": "起始数值",
      "default": "0",
      "type": "起始数值"
    },
    "target-num": {
      "value": "",
      "description": "目标数值",
      "default": "-",
      "type": "目标数值"
    },
    "text-list": {
      "value": "",
      "description": "内容数组，用于翻转非数字内容",
      "default": "[]",
      "type": "内容数组，用于翻转非数字内容"
    },
    "duration": {
      "value": "",
      "description": "动画时长，单位为秒",
      "default": "2",
      "type": "动画时长，单位为秒"
    },
    "direction": {
      "value": "",
      "description": "文本翻滚方向，值为 `down` 和 `up`",
      "default": "down",
      "type": "文本翻滚方向，值为 down 和 up"
    },
    "auto-start": {
      "value": "",
      "description": "是否自动开始动画",
      "default": "true",
      "type": "是否自动开始动画"
    },
    "stop-order": {
      "value": "",
      "description": "各个数位动画停止先后顺序，值为 `ltr` 和 `rtl`",
      "default": "ltr",
      "type": "各个数位动画停止先后顺序，值为 ltr 和 rtl"
    },
    "height": {
      "value": "",
      "description": "数字高度，单位为 `px`",
      "default": "40",
      "type": "数字高度，单位为 px"
    }
  },
  "methods": [{ "name": "start", "description": "开始动画", "params": "-" }, { "name": "reset", "description": "重置动画", "params": "-" }],
  "typeDetail": {},
  "events": [],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/rolling-text"
