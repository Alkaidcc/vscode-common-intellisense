{
  "name": "VanRollText",
  "props": {
    "start-num": {
      "value": "",
      "description": "起始数值",
      "default": "0",
      "type": "number"
    },
    "target-num": {
      "value": "",
      "description": "目标数值",
      "default": "-",
      "type": "number"
    },
    "text-list": {
      "value": "",
      "description": "内容数组，用于翻转非数字内容",
      "default": "[]",
      "type": "string[]"
    },
    "duration": {
      "value": "",
      "description": "动画时长，单位为秒",
      "default": "2",
      "type": "number"
    },
    "direction": {
      "value": "",
      "description": "文本翻滚方向，值为 `down` 和 up",
      "default": "down",
      "type": "string"
    },
    "auto-start": {
      "value": "",
      "description": "是否自动开始动画",
      "default": "true",
      "type": "boolean"
    },
    "stop-order": {
      "value": ["ltr", "rtl"],
      "description": "各个数位动画停止先后顺序，值为 `ltr` 和 rtl",
      "default": "ltr",
      "type": "string"
    },
    "height": {
      "value": "",
      "description": "数字高度，单位为 px",
      "default": "40",
      "type": "number"
    }
  },
  "methods": [{ "name": "start", "description": "开始动画", "params": "-" }, { "name": "reset", "description": "重置动画", "params": "-" }],
  "typeDetail": {},
  "events": [],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/rolling-text"
}
