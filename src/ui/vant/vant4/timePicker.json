{
  "name": "VanTimePicker",
  "props": {
    "v-model": {
      "value": "",
      "description": "当前选中的时间",
      "default": "-",
      "type": "string[]",
      "required": true
    },
    ":columns-type": {
      "value": "",
      "description": "选项类型，由 `hour`、`minute` 和 `second` 组成的数组",
      "default": "['hour', 'minute']",
      "type": "string[]"
    },
    "min-hour": {
      "value": "",
      "description": "可选的最小小时",
      "default": "0",
      "type": "number | string"
    },
    "max-hour": {
      "value": "",
      "description": "可选的最大小时",
      "default": "23",
      "type": "number | string"
    },
    "min-minute": {
      "value": "",
      "description": "可选的最小分钟",
      "default": "0",
      "type": "number | string"
    },
    "max-minute": {
      "value": "",
      "description": "可选的最大分钟",
      "default": "59",
      "type": "number | string"
    },
    "min-second": {
      "value": "",
      "description": "可选的最小秒数",
      "default": "0",
      "type": "number | string"
    },
    "max-second": {
      "value": "",
      "description": "可选的最大秒数",
      "default": "59",
      "type": "number | string"
    },
    "min-time": {
      "value": "",
      "description": "可选的最小时间，格式参考 `07:40:00`，使用时 `min-hour` `min-minute` `min-second` 不会生效",
      "default": "-",
      "type": "string"
    },
    "max-time": {
      "value": "",
      "description": "可选的最大时间，格式参考 `10:20:00`，使用时 `max-hour` `max-minute` `max-second` 不会生效",
      "default": "-",
      "type": "string"
    },
    "title": {
      "value": "",
      "description": "顶部栏标题",
      "default": "''",
      "type": "string"
    },
    "confirm-button-text": {
      "value": "",
      "description": "确认按钮文字",
      "default": "确认",
      "type": "string"
    },
    "cancel-button-text": {
      "value": "",
      "description": "取消按钮文字",
      "default": "取消",
      "type": "string"
    },
    "show-toolbar": {
      "value": "",
      "description": "是否显示顶部栏",
      "default": "true",
      "type": "boolean"
    },
    "loading": {
      "value": "",
      "description": "是否显示加载状态",
      "default": "false",
      "type": "boolean"
    },
    "readonly": {
      "value": "",
      "description": "是否为只读状态，只读状态下无法切换选项",
      "default": "false",
      "type": "boolean"
    },
    ":filter": {
      "value": "",
      "description": "选项过滤函数",
      "default": "-",
      "type": "(type: string, options: PickerOption[], values: string[]) => PickerOption[]"
    },
    "formatter": {
      "value": "",
      "description": "选项格式化函数",
      "default": "-",
      "type": "(type: string, option: PickerOption) => PickerOption"
    },
    "option-height": {
      "value": "",
      "description": "选项高度，支持 `px` `vw` `vh` `rem` 单位，默认 px",
      "default": "44",
      "type": "number | string"
    },
    "visible-option-num": {
      "value": "",
      "description": "可见的选项个数",
      "default": "6",
      "type": "number | string"
    },
    "swipe-duration": {
      "value": "",
      "description": "快速滑动时惯性滚动的时长，单位 ms",
      "default": "1000",
      "type": "number | string"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "confirm",
      "description": "点击完成按钮时触发",
      "params": "点击完成按钮时触发"
    },
    {
      "name": "cancel",
      "description": "点击取消按钮时触发",
      "params": "点击取消按钮时触发"
    },
    {
      "name": "change",
      "description": "选项改变时触发",
      "params": "选项改变时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/time-picker"
}
