{
  "name": "VanCheckboxGroup",
  "props": {
    "v-model": {
      "value": "",
      "description": "所有选中项的标识符",
      "default": "-",
      "type": "any[]",
      "required": true
    },
    "disabled": {
      "value": "",
      "description": "是否禁用所有复选框",
      "default": "false",
      "type": "boolean"
    },
    "max": {
      "value": "",
      "description": "最大可选数，`0` 为无限制",
      "default": "0",
      "type": "number | string"
    },
    "direction": {
      "value": ["vertical", "horizontal"],
      "description": "排列方向，可选值为 horizontal",
      "default": "vertical",
      "type": "string"
    },
    "icon-size": {
      "value": "",
      "description": "所有复选框的图标大小，默认单位为 px",
      "default": "20px",
      "type": "number | string"
    },
    "checked-color": {
      "value": "",
      "description": "所有复选框的选中状态颜色",
      "default": "#1989fa",
      "type": "string"
    },
    "shape": {
      "value": ["round", "square"],
      "description": "形状，可选值为 square",
      "default": "round",
      "type": "string"
    }
  },
  "methods": [{ "name": "toggleAll", "description": "切换所有复选框，传 `true` 为选中，`false` 为取消选中，不传参为取反", "params": "options?: boolean | object" }],
  "typeDetail": {},
  "events": [
    {
      "name": "change",
      "description": "当绑定值变化时触发的事件",
      "params": "当绑定值变化时触发的事件"
    }
  ],
  "link": "https://vant-docs.netlify.app/#/zh-CN/checkbox"
}
