{
  "name": "VanCascader",
  "props": {
    "v-model": {
      "value": "",
      "description": "选中项的值",
      "default": "-",
      "type": "string | number"
    },
    "title": {
      "value": "",
      "description": "顶部标题",
      "default": "-",
      "type": "string"
    },
    ":options": {
      "value": "",
      "description": "可选项数据源",
      "default": "[]",
      "type": "CascaderOption[]"
    },
    "placeholder": {
      "value": "",
      "description": "未选中时的提示文案",
      "default": "请选择",
      "type": "string"
    },
    "active-color": {
      "value": "",
      "description": "选中状态的高亮颜色",
      "default": "#1989fa",
      "type": "string"
    },
    "swipeable": {
      "value": "",
      "description": "是否开启手势左右滑动切换",
      "default": "true",
      "type": "boolean"
    },
    "closeable": {
      "value": "",
      "description": "是否显示关闭图标",
      "default": "true",
      "type": "boolean"
    },
    "show-header": {
      "value": "",
      "description": "是否展示标题栏",
      "default": "true",
      "type": "boolean"
    },
    "close-icon": {
      "value": "",
      "description": "关闭图标名称或图片链接，等同于 Icon 组件的 [name 属性](#/zh-CN/icon#props)",
      "default": "cross",
      "type": "string"
    },
    "field-names": {
      "value": "",
      "description": "自定义 `options` 结构中的字段",
      "default": "{ text: 'text', value: 'value', children: 'children' }",
      "type": "CascaderFieldNames"
    }
  },
  "methods": [],
  "typeDetail": {
    "CascaderOption": [
      {
        "name": "text",
        "description": "选项文字（必填）",
        "type": "string"
      },
      {
        "name": "value",
        "description": "选项对应的值（必填）",
        "type": "string | number"
      },
      {
        "name": "color",
        "description": "选项文字颜色",
        "type": "string"
      },
      {
        "name": "children",
        "description": "子选项列表",
        "type": "CascaderOption[]"
      },
      {
        "name": "disabled",
        "description": "是否禁用选项",
        "type": "boolean"
      },
      {
        "name": "className",
        "description": "为对应列添加额外的 class",
        "type": "string | Array | object"
      }
    ]
  },
  "events": [
    {
      "name": "change",
      "description": "选中项变化时触发",
      "params": "选中项变化时触发"
    },
    {
      "name": "finish",
      "description": "全部选项选择完成后触发",
      "params": "全部选项选择完成后触发"
    },
    {
      "name": "close",
      "description": "点击关闭图标时触发",
      "params": "点击关闭图标时触发"
    },
    {
      "name": "click-tab",
      "description": "点击标签时触发",
      "params": "点击标签时触发"
    }
  ],
  "link": "https://vant-docs.netlify.app/#/zh-CN/cascader"
}
