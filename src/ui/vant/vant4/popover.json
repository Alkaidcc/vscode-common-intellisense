{
  "name": "VanPopover",
  "props": {
    "v-model:show": {
      "value": "",
      "description": "是否展示气泡弹出层",
      "default": "false",
      "type": "boolean"
    },
    ":actions": {
      "value": "",
      "description": "选项列表",
      "default": "[]",
      "type": "PopoverAction[]"
    },
    "actions-direction": {
      "value": ["vertical", "horizontal"],
      "description": "选项列表的排列方向，可选值为 horizontal",
      "default": "vertical",
      "type": "PopoverActionsDirection"
    },
    "placement": {
      "value": "",
      "description": "弹出位置",
      "default": "bottom",
      "type": "PopoverPlacement"
    },
    "theme": {
      "value": ["light", "dark"],
      "description": "主题风格，可选值为 dark",
      "default": "light",
      "type": "PopoverTheme"
    },
    "trigger": {
      "value": ["click", "manual"],
      "description": "触发方式，可选值为 manual",
      "default": "click",
      "type": "PopoverTrigger"
    },
    "duration": {
      "value": "",
      "description": "动画时长，单位秒，设置为 0 可以禁用动画",
      "default": "0.3",
      "type": "number | string"
    },
    "offset": {
      "value": "",
      "description": "出现位置的偏移量",
      "default": "[0, 8]",
      "type": "[number, number]"
    },
    "overlay": {
      "value": "",
      "description": "是否显示遮罩层",
      "default": "false",
      "type": "boolean"
    },
    "overlay-class": {
      "value": "",
      "description": "自定义遮罩层类名",
      "default": "-",
      "type": "string | Array | object"
    },
    "overlay-style": {
      "value": "",
      "description": "自定义遮罩层样式",
      "default": "-",
      "type": "object"
    },
    "show-arrow": {
      "value": "",
      "description": "是否展示小箭头",
      "default": "true",
      "type": "boolean"
    },
    "close-on-click-action": {
      "value": "",
      "description": "是否在点击选项后关闭",
      "default": "true",
      "type": "boolean"
    },
    "close-on-click-outside": {
      "value": "",
      "description": "是否在点击外部元素后关闭菜单",
      "default": "true",
      "type": "boolean"
    },
    "close-on-click-overlay": {
      "value": "",
      "description": "是否在点击遮罩层后关闭菜单",
      "default": "true",
      "type": "boolean"
    },
    "teleport": {
      "value": "",
      "description": "指定挂载的节点，等同于 Teleport 组件的 [to 属性](https://cn.vuejs.org/api/built-in-components.html#teleport)",
      "default": "body",
      "type": "string | Element"
    },
    "icon-prefix": {
      "value": "",
      "description": "图标类名前缀，等同于 Icon 组件的 [class-prefix 属性](#/zh-CN/icon#props)",
      "default": "van-icon",
      "type": "string"
    }
  },
  "methods": [],
  "typeDetail": {
    "PopoverAction": [
      {
        "name": "text",
        "description": "选项文字",
        "type": "string"
      },
      {
        "name": "icon",
        "description": "文字左侧的图标，支持传入图标名称或图片链接，等同于 Icon 组件的 [name 属性](#/zh-CN/icon#props)",
        "type": "string"
      },
      {
        "name": "color",
        "description": "选项文字颜色",
        "type": "string"
      },
      {
        "name": "disabled",
        "description": "是否为禁用状态",
        "type": "boolean"
      },
      {
        "name": "className",
        "description": "为对应选项添加额外的类名",
        "type": "string | Array | object"
      }
    ]
  },
  "events": [
    {
      "name": "select",
      "description": "点击选项时触发",
      "params": "点击选项时触发"
    },
    {
      "name": "open",
      "description": "打开菜单时触发",
      "params": "打开菜单时触发"
    },
    {
      "name": "close",
      "description": "关闭菜单时触发",
      "params": "关闭菜单时触发"
    },
    {
      "name": "opened",
      "description": "打开菜单且动画结束后触发",
      "params": "打开菜单且动画结束后触发"
    },
    {
      "name": "closed",
      "description": "关闭菜单且动画结束后触发",
      "params": "关闭菜单且动画结束后触发"
    },
    {
      "name": "click-overlay",
      "description": "点击遮罩层时触发",
      "params": "点击遮罩层时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/popover"
}
