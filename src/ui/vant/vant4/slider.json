{
  "name": "VanSlider",
  "props": {
    "v-model": {
      "value": "",
      "description": "当前进度百分比，在双滑块模式下为数组格式",
      "default": "0",
      "type": "number | [number, number]"
    },
    "max": {
      "value": "",
      "description": "最大值",
      "default": "100",
      "type": "number | string"
    },
    "min": {
      "value": "",
      "description": "最小值",
      "default": "0",
      "type": "number | string"
    },
    "step": {
      "value": "",
      "description": "步长",
      "default": "1",
      "type": "number | string"
    },
    "bar-height": {
      "value": "",
      "description": "进度条高度，默认单位为 `px`",
      "default": "2px",
      "type": "number | string"
    },
    "button-size": {
      "value": "",
      "description": "滑块按钮大小，默认单位为 `px`",
      "default": "24px",
      "type": "number | string"
    },
    "active-color": {
      "value": "",
      "description": "进度条激活态颜色",
      "default": "#1989fa",
      "type": "string"
    },
    "inactive-color": {
      "value": "",
      "description": "进度条非激活态颜色",
      "default": "#e5e5e5",
      "type": "string"
    },
    "range": {
      "value": "",
      "description": "是否开启双滑块模式",
      "default": "false",
      "type": "boolean"
    },
    "reverse": {
      "value": "",
      "description": "是否将进度条反转",
      "default": "false",
      "type": "boolean"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用滑块",
      "default": "false",
      "type": "boolean"
    },
    "readonly": {
      "value": "",
      "description": "是否为只读状态，只读状态下无法修改滑块的值",
      "default": "false",
      "type": "boolean"
    },
    "vertical": {
      "value": "",
      "description": "是否垂直展示",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [],
  "typeDetail": {},
  "events": [
    {
      "name": "update:model-value",
      "description": "进度变化时实时触发",
      "params": "进度变化时实时触发"
    },
    {
      "name": "change",
      "description": "进度变化且结束拖动后触发",
      "params": "进度变化且结束拖动后触发"
    },
    {
      "name": "drag-start",
      "description": "开始拖动时触发",
      "params": "开始拖动时触发"
    },
    {
      "name": "drag-end",
      "description": "结束拖动时触发",
      "params": "结束拖动时触发"
    }
  ],
  "link": "https://vant-contrib.gitee.io/vant/#/zh-CN/slider"
}
